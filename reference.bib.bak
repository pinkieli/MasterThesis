@Article{Fung1997,
  author                     = {Fung, TC},
  title                      = {A precise time-step integration method by step-response and impulsive-response matrices for dynamic problems},
  journal                    = {International Journal for Numerical Methods in Engineering},
  year                       = {1997},
  volume                     = {40},
  number                     = {24},
  pages                      = {4501-4527},
  month                      = {DEC 30},
  issn                       = {0029-5981},
  abstract                   = {In this paper, a precise time-step integration method for dynamic
   problems is presented. The second-order differential equations for
   dynamic problems are manipulated directly. A general damping matrix is
   considered. The transient responses are expressed in terms of the
   steady-state responses, the given initial conditions and the
   step-response and impulsive-response matrices. The steady-state
   responses for various types of excitations are readily obtainable. The
   computation of the step-response and impulsive-response matrices and
   their time derivatives are studied in this paper. A direct computation
   of these matrices using the Taylor series solutions is not efficient
   when the time-step size Delta t is not small. In this paper, the
   recurrence formulae relating the response matrices at t = Delta t to
   those at t = Delta t/2 are constructed. A recursive procedure is
   proposed to evaluate these matrices at t = Delta t from the matrices at
   t = Delta t/2(m). The matrices at t = Delta t/2(m) are obtained from the
   Taylor series solutions. To improve the computational efficiency, the
   relations between the response matrices and their time derivatives are
   investigated. In addition, these matrices are expressed in terms of two
   symmetric matrices that can also be evaluated recursively. Besides, from
   the physical point of view, these matrices should be banded for small
   Delta t. Both the stability and accuracy characteristics of the present
   algorithm are studied. Three numerical examples are used to illustrate
   the highly precise and stable algorithm. (C) 1997 John Wiley \& Sons,
   Ltd.},
  affiliation                = {Fung, TC (Reprint Author), NANYANG TECHNOL UNIV,SCH CIVIL \& STRUCT ENGN,NANYANG AVE,SINGAPORE 639798,SINGAPORE.},
  cited-references           = {Bathe KJ, 1982, FINITE ELEMENT PROCE. CAUGHEY TK, 1965, J APPL MECH, V32, P583. Clough R. W., 1993, DYNAMICS STRUCTURES. COOK RD, 1989, CONCEPTS APPLICATION. Frazer RA, 1957, ELEMENTARY MATRICES. Hairer E., 1991, SOLVING ORDINARY DIF, VII. Hairer E., 1987, SOLVING ORDINARY DIF. Hughes T. J. R., 1987, FINITE ELEMENT METHO. IRONS BM, 1971, P 2 C MATR METH STRU, P245. Isaacson E., 1966, ANAL NUMERICAL METHO. LEUNG AYT, 1986, J SOUND VIB, V106, P145, DOI 10.1016/S0022-460X(86)80178-X. Leung A Y T, 1993, DYNAMIC STIFFNESS SU. LEUNG AYT, 1985, EARTHQUAKE ENG STRUC, V13, P677, DOI 10.1002/eqe.4290130509. ZHONG WX, 1994, P I MECH ENG C-J MEC, V208, P427.},
  doi                        = {10.1002/(SICI)1097-0207(19971230)40:24<4501::AID-NME266>3.0.CO;2-U},
  file                       = {:Func T C\\Fung1997e.pdf:PDF;:Func T C\\Fung1997c.pdf:PDF;:Func T C\\Fung1997b.pdf:PDF;:Func T C\\Fung1997a.pdf:PDF},
  groups                     = {Dynamic equation numerical methods},
  keywords                   = {relevant, printed},
  number-of-cited-references = {14},
  researcherid-numbers       = {Fung, Tat Ching/A-3775-2011},
  times-cited                = {34},
  unique-id                  = {ISI:A1997YK95000003},
}

@Article{Kim1997,
  author                     = {Kim, SJ and Cho, JY and Kim, WD},
  title                      = {From the trapezoidal rule to higher-order accurate and unconditionally stable time-integration method for structural dynamics},
  journal                    = {Computer Methods in Applied Mechanics and Engineering},
  year                       = {1997},
  volume                     = {{149}},
  number                     = {{1-4}},
  pages                      = {{73-88}},
  month                      = {{OCT}},
  issn                       = {{0045-7825}},
  note                       = {{Interdisciplinary Symposium on Advances in Computational Mechanics in Honor of Professor John Tinsley Oden, AUSTIN, TX, JAN 12-15, 1997}},
  abstract                   = {{Since the trapezoidal rule was originally proposed as the constant
   average acceleration method by Newmark, it has been modified to various
   forms. However, most of the modifications were limited to its numerical
   coefficients and, as a result, little change in the characteristics was
   obtained like numerical dissipation with the loss of accuracy. In this
   work, the constant average acceleration, which is a fundamental feature
   of the trapezoidal rule, is investigated and generalized. Using the
   generalization of average acceleration concept, a higher-order accurate
   and unconditionally stable time-integration method is developed.
   The stability analysis is carried out for a system of single, degree of
   freedom. The analysis proves that the present method is unconditionally
   stable. To observe the accuracy of the method, several numerical tests
   are performed and the results are compared with the results from the
   trapezoidal rule and the exact solution. From the numerical tests, it
   has been known that the present method has a higher order accuracy.}},
  affiliation                = {{Kim, SJ (Reprint Author), SEOUL NATL UNIV, DEPT AEROSP ENGN, KWANAK KU, SAN 56-1, SEOUL 151742, SOUTH KOREA. PUSAN NATL UNIV, DEPT AEROSP ENGN, KUMJUNG KU, PUSAN 609735, SOUTH KOREA.}},
