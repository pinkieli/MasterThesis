  keywords  = {NUMERICAL MANIFOLD METHOD;DISCONTINUOUS DEFORMATION ANALYSIS;PIPE;ELBOW ELEMENT;COMPUTATIONAL MECHANICS;CRACK-GROWTH;PARTITION;SPHERES;DYNAMICS;FEM},
  owner     = {Pinkie.li},
