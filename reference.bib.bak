@Article{Niemi1995,
  author    = {Niemi, Jarmo},
  title     = {A0 – Stable family of single step methods for semidiscretized parabolic problems},
  journal   = {International Journal for Numerical Methods in Engineering},
  year      = {1995},
  volume    = {38},
  number    = {16},
  pages     = {2829--2836},
  issn      = {1097-0207},
  abstract  = {A general family of single-step methods for use in first-order ODEs is presented. This family of methods, called SS3-methods, embraces generalized trapezoidal methods, SS21-methods, α-methods and many other well-known methods, thus providing a good basis for comparison of these methods. Corresponding parameter values of SS3 methods for these methods are presented. The analysis of SS3-methods is restricted to linear and symmetric systems. Stability, convergence, accuracy, numerical dissipation and overshoot of the methods are considered. The conditions for algorithm parameters to exhibit good characteristics in these respects are given. It is shown that the optimal subfamily of SS3-methods coincides with α-methods.},
  doi       = {10.1002/nme.1620381609},
  groups    = {W.L.WOOD},
  keywords  = {parabolic problems, numerical time integration},
  publisher = {John Wiley \& Sons, Ltd},
