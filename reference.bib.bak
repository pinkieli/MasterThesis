  journal   = {Automatica},
  year      = {2014},
  volume    = {50},
  number    = {9},
  pages     = {2234 - 2244},
  issn      = {00051098},
  note      = {Adaptive dynamic programming;Collocation method;Hamilton Jacobi Bellman equation;Hamilton-Jacobi-Bellman equations;Iteration procedure;Optimal feedback control;Quadratic programming problems;Time-dependent coefficients;},
  language  = {English},
  abstract  = {This paper presents a sparse collocation method for solving the time-dependent Hamilton-Jacobi-Bellman (HJB) equation associated with the continuous-time optimal control problem on a fixed, finite time-horizon with integral cost functional. Through casting the problem in a recursive framework using the value-iteration procedure, the value functions of every iteration step is approximated with a time-varying multivariate simplex B-spline on a certain state domain of interest. In the collocation scheme, the time-dependent coefficients of the spline function are further approximated with ordinary univariate B-splines to yield a discretization for the value function fully in terms of piece-wise polynomials. The B-spline coefficients are determined by solving a sequence of highly sparse quadratic programming problems. The proposed algorithm is demonstrated on a pair of benchmark example problems. Simulation results indicate that the method can yield increasingly more accurate approximations of the value function by refinement of the triangulation. &copy; 2014 Elsevier Ltd. All rights reserved.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Dynamic programming},
  keywords  = {Continuous time systems;Interpolation;Iterative methods;Optimal control systems;Quadratic programming;Recursive functions;Ship propellers;Splines;},
  url       = {http://dx.doi.org/10.1016/j.automatica.2014.07.012},
}

@Article{Rus2010,
  author    = {Rus, Francisco and Villatoro, Francisco R.},
  title     = {Time-stepping in Petrov-Galerkin methods based on cubic B-splines for compactons},
  journal   = {Applied Mathematics and Computation},
  year      = {2010},
  volume    = {217},
  number    = {6},
  pages     = {2788 - 2797},
  issn      = {00963003},
  note      = {Compactons;Computational costs;Cubic B-splines;Fourth-order;Numerical results;Petrov-Galerkin methods;Rosenau-Hyman equation;Second orders;Self-similarities;Time integration;Time integration methods;Time stepping method;Time-stepping;Wavefront velocity;},
  language  = {English},
  abstract  = {Four numerical methods with first- to fourth-order of accuracy have been developed for the time integration of the Rosenau-Hyman K(2, 2) equation. The error in the solution and the invariants for the propagation of one-compacton, and the stability in collisions among compactons have been studied using these methods. Numerically-induced radiation has also been characterized by means of wavefront velocity and wavefront amplitude, showing that the self-similarity of the radiation wavepackets observed in the numerical results is a consequence of the time-stepping method. Among the four methods studied in this paper, the best results in terms of accuracy, computational cost, and stability have been obtained by means of using the second-order time integration method. &copy; 2010 Elsevier Inc. All rights reserved.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
