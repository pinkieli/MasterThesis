  url       = {http://dx.doi.org/10.1016/S0045-7949(01)00111-0},
}

@Article{Jacquelin2017,
  author    = {Jacquelin, E. and Dessombz, O. and Sinou, J.-J. and Adhikari, S. and Friswell, M.I.},
  title     = {Polynomial chaos-based extended Pade expansion in structural dynamics},
  journal   = {International Journal for Numerical Methods in Engineering},
  year      = {2017},
  volume    = {111},
  number    = {12},
  pages     = {1170 - 1191},
  issn      = {00295981},
  note      = {Approximants;Frequency response functions;Polynomial chaos expansion;Polynomial chaos representation;Probability density function (pdf);Random dynamical system;Random modes;Two-degree-of-freedom system;},
  language  = {English},
  abstract  = {The response of a random dynamical system is totally characterized by its probability density function (pdf). However, determining a pdf by a direct approach requires a high numerical cost; similarly, surrogate models such as direct polynomial chaos expansions are not generally efficient, especially around the eigenfrequencies of the dynamical system. In the present study, a new approach based on Pad&eacute; approximants to obtain moments and pdf of the dynamic response in the frequency domain is proposed. A key difference between the direct polynomial chaos representation and the Pad&eacute; representation is that the Pad&eacute; approach has polynomials in both numerator and denominator. For frequency response functions, the denominator plays a vital role as it contains the information related to resonance frequencies, which are uncertain. A Galerkin approach in conjunction with polynomial chaos is proposed for the Pad&eacute; approximation. Another physics-based approach, utilizing polynomial chaos expansions of the random eigenmodes, is proposed and compared with the proposed Pad&eacute; approach. It is shown that both methods give accurate results even if a very low degree of the polynomial expansion is used. The methods are demonstrated for two degree-of-freedom system with one and two uncertain parameters. Copyright &copy; 2016 John Wiley &amp; Sons, Ltd. Copyright &copy; 2016 John Wiley & Sons, Ltd.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {Dynamic equation numerical methods},
  key       = {Polynomial approximation},
  keywords  = {Degrees of freedom (mechanics);Dynamical systems;Frequency domain analysis;Frequency response;Polynomials;Probability density function;Structural dynamics;Uncertainty analysis;},
  owner     = {Pinkie.li},
  timestamp = {2017-09-15},
  url       = {http://dx.doi.org/10.1002/nme.5497},
}

@Article{Goudreau1973,
  author    = {Goudreau, G. L. and Taylor, R. L.},
  title     = {Evaluation of numerical methods in elastodynamics},
  journal   = {Computer Methods in Applied Mechanics \& Engineering},
  year      = {1973},
  volume    = {2},
  number    = {1},
  pages     = {69–97},
  abstract  = {Abstract One-step algorithms are compared for the direct time integration of the spatially discretized equations of linear elastodynamics. Explicit and implicit methods are evaluated for stability and accuracy in terms of the frequency spectrum of the continuum as well as discrete solution. The closed form of the discrete spectrum of a bar and beam are given to illustrate inherent errors of a spatial discretization. Additional errors of a direct time integration scheme are interpreted in terms of the discrete modes. The Newmark family of second-order difference approximations is found superior to either the original or extended Wilson methods, and to that of Houboult. Less than half as many time steps are required for comparable accuracy. If an upper bound on the highest frequency of a system is found, an optimum algorithm for a given time step can be found, rather than the usual selection of a time step for a given algorithm. The algorithmic damping admitted by the Newmark family can be controlled as a 鈥渓inear artificial viscosity鈥 to smooth shock discontinuities without the excessive damping of the Wilson and Houboult methods. The implications of these algorithms in wave propagation problems are illustrated.},
  groups    = {Dynamic equation numerical methods},
  owner     = {Pinkie.li},
  timestamp = {2017-10-06},
}

