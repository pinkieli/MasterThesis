  url      = {http://www.sciencedirect.com/science/article/pii/S0168927496000438},
}

@Article{Butcher1997,
  author   = {J.C. Butcher},
  title    = {An introduction to “Almost Runge-Kutta” methods},
  journal  = {Applied Numerical Mathematics},
  year     = {1997},
  volume   = {24},
  number   = {2–3},
  pages    = {331 - 342},
  issn     = {0168-9274},
  note     = {Second International Conference on the Numerical Solution of Volterra and Delay Equations},
  abstract = {In this paper we consider numerical methods which differ from Runge-Kutta methods in that they are multivalue and have a greater stage order than is possible with explicit Runge-Kutta methods. On the other hand they share many other properties with one-step methods. In particular, they have stability properties as for Runge-Kutta methods and it is easy to start the methods and to change stepsize. Because of the high stage order, continuous output is easier to achieve than for Runge-Kutta methods and this gives them a potential for the solution of delay problems as well as for ordinary differential equations. These new methods will be known as “ARK” or “Almost Runge-Kutta” methods. Even though they exist for orders at least as high as 6, we will concentrate here on methods of order 4, for which we present four sample methods. For one of these methods, we will present some numerical results for some initial value ordinary differential equation problems and some delay differential equations, illustrating various features of the new methods. },
  doi      = {http://dx.doi.org/10.1016/S0168-9274(97)00030-5},
  file     = {:paper\\An-introduction-to-Almost-Runge-Kutta-methods_Butcher_Applied-Numerical-Mathematics.pdf:PDF},
  groups   = { General linear method},
  keywords = {Runge-Kutta methods, Multivalue methods, Stage order, Continuous output, Error estimation },
  url      = {http://www.sciencedirect.com/science/article/pii/S0168927497000305},
}

@Article{Butcher1997a,
  author   = {J.C. Butcher and P. Chartier},
  title    = {A generalization of singly-implicit Runge-Kutta methods},
  journal  = {Applied Numerical Mathematics},
  year     = {1997},
  volume   = {24},
  number   = {2–3},
  pages    = {343 - 350},
  issn     = {0168-9274},
  note     = {Second International Conference on the Numerical Solution of Volterra and Delay Equations},
  abstract = {The concept of effective order allows for the possibility that the result computed in a Runge-Kutta step is an approximation to some quantity more general than the actual solution at a step point. This generalization is applied here to singly-implicit methods. The limitation that requires severe and inconvenient restrictions on the abscissae in the method is removed and all that is now needed is that the abscissae be distinct. Finally, the construction of several low order methods will be detailed. },
  doi      = {http://dx.doi.org/10.1016/S0168-9274(97)00031-7},
  file     = {:paper\\A-generalization-of-singly-implicit-Runge-Kutta-methods_Butcher_Applied-Numerical-Mathematics.pdf:PDF},
  groups   = {General linear method},
  keywords = {Effective order, Runge-Kutta methods, Singly-implicit methods },
