  pages     = {1325 - 1328},
  issn      = {00988847},
  language  = {English},
  abstract  = {It seems that the explicit KR-&alpha; method (KRM) is promising for the step-by-step integration because it simultaneously integrates unconditional stability, explicit formulation, and numerical dissipation together. It was shown that KRM can inherit the numerical dispersion and energy dissipation properties of the generalized-&alpha; method (GM) for a linear elastic system, and it reduces to CR method (CRM) if &rho;<inf>&infin;</inf>=1is adopted, where &rho;<inf>&infin;</inf>is the spectral radius of the amplification matrix of KRM as the product of the natural frequency and the step size tends to infinity. However, two unusual properties were found for KRM and CRM, and they might limit their application to solve either linear elastic or nonlinear systems. One is the lack of capability to capture the structural nonlinearity, and the other is that it is unable to realistically reflect the dynamic loading. &copy; 2014 John Wiley & Sons, Ltd.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  file      = {:paper\\Unconditionally Stable Explicit Method\\Discussion of paper ‘Development of a family of unconditionally stable explicit direct integration algorithms with controllable numerical energy dissipati....pdf:PDF},
  groups    = {Unconditionally Stable Explicit Method},
  url       = {http://dx.doi.org/10.1002/eqe.2515},
}

@Article{Chang2010,
