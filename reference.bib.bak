  unique-id            = {{ISI:000077349500004}},
}

@Article{Taylor1998b,
  author               = {Taylor, RL and Zienkiewicz, OC and Onate, E},
  title                = {{A hierarchical finite element method based on the partition of unity}},
  journal              = {{COMPUTER METHODS IN APPLIED MECHANICS AND ENGINEERING}},
  year                 = {{1998}},
  volume               = {{152}},
  number               = {{1-2}},
