@Article{Feriani2000,
  author                     = {Feriani, A and Perotti, F and Simoncini, V},
  title                      = {{Iterative system solvers for the frequency analysis of linear mechanical systems}},
  journal                    = {{COMPUTER METHODS IN APPLIED MECHANICS AND ENGINEERING}},
  year                       = {{2000}},
  volume                     = {{190}},
  number                     = {{13-14}},
  pages                      = {{1719-1739}},
  issn                       = {{0045-7825}},
  abstract                   = {{The paper deals with the numerical treatment of the direct frequency
   domain (DFD) analysis of linear mechanical systems. Attention is mainly
   focused on the solution of the complex system of equations needed for
   each analyzed frequency. Strategies for transforming the system into the
   ``shifted{''} form (T - sigmaI)z = d are proposed and discussed, a where
   rr is related to the frequency. For two formulations of the shifted
   system the performance of some Krylov sub-space iterative solvers is
   tested and compared to that of a multi-frontal direct method. Advantage
   is taken of the shifted form in solving simultaneously a large number of
   systems resulting from different values of the shift (frequency).
   Numerical experiments on some prototype structural dynamics problems are
   reported. the results shown demonstrate how the devised strategies for
   the iterative solution can outperform, in many cases, the direct solver.
   (C) 2000 Elsevier Science B.V. All rights reserved.}},
  affiliation                = {{Perotti, F (Reprint Author), Politecn Milan, Dipartimento Ingn Strutturale, Piazza Leonardo da Vinci 32, I-20133 Milan, Italy. Politecn Milan, Dipartimento Ingn Strutturale, I-20133 Milan, Italy. Univ Brescia, Dipartimento Ingn Civile, I-25123 Brescia, Italy. CNR, Ist Anal Numer, I-27100 Pavia, Italy.}},
