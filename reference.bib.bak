  groups    = {Matrix exponential},
  key       = {Matrix algebra},
  keywords  = {Approximation algorithms;Chaos theory;Error analysis;Least squares approximations;Vectors;},
  url       = {http://dx.doi.org/10.1016/j.cam.2011.11.018},
}

