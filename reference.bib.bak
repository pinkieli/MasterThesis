@InProceedings{Kanapady2013,
  author    = {Kanapady, Ramdev and Srinivasan, Siddharth and Tamma, Kumar},
  title     = {A Time Discontinuous Formulations Preserving a Consistent High-Order of Accuracy for Nonlinear/Linear Structural Dynamics},
  booktitle = {Aiaa/asme/asce/ahs/asc Structures, Structural Dynamics, and Materials Conference},
