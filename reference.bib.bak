@Article{Muǧan2003,
  author    = {Mugan, Ata},
  title     = {Discrete equivalent time integration methods for transient analysis},
  journal   = {International Journal for Numerical Methods in Engineering},
  year      = {2003},
  volume    = {57},
  number    = {14},
  pages     = {2043--2075},
  month     = {8},
  issn      = {1097-0207},
  abstract  = {It was recently shown in a series of papers that the frequency response of temporally discretized finite element equations and, consequently, the achievable accuracy cannot be manipulated independently in different frequency ranges. In addition, there exist limitations on the achievable accuracy of a time integration method no matter what the order of accuracy of the method is. Motivated by this fact, a family of time integration methods is derived in the time domain based on the principle that the exact solution of the semidiscrete equation of a system and the solution of the time integration method match at discrete time steps. It is necessary to pursue an exact match at discrete time steps, i.e. discrete equivalence, since the solutions of semidiscrete equations are obtained only at the time steps. Two time integration methods, that are exact at the time steps, are obtained based on the impulse and ramp response invariance principles. Numerical examples are presented to show the advantage of the proposed methods and to compare the performance of them with the performance of some popular methods. Copyright © 2003 John Wiley & Sons, Ltd.},
  doi       = {10.1002/nme.755},
  file      = {:paper\\Control theory\\Mu-an-2003-International_Journal_for_Numerical_Methods_in_Engineering.pdf:PDF},
  groups    = {Control theory},
  publisher = {Wiley Online Library},
  url       = {http:https://dx.doi.org/10.1002/nme.755},
}

@Article{Zhou2007,
  author         = {Zhou, Jun and Zhou, Youhe},
  title          = {A new simple method of implicit time integration for dynamic problems of engineering structures},
  journal        = {Acta Mechanica Sinica},
  year           = {2007},
  volume         = {23},
  number         = {1},
  pages          = {91--99},
  issn           = {1614-3116},
  abstract       = {This paper presents a new simple method of implicit time integration with two control parameters for solving initial-value problems of dynamics such that its accuracy is at least of order two along with the conditional and unconditional stability regions of the parameters. When the control parameters in the method are optimally taken in their regions, the accuracy may be improved to reach of order three. It is found that the new scheme can achieve lower numerical amplitude dissipation and period dispersion than some of the existing methods, e.g. the Newmark method and Zhai's approach, when the same time step size is used. The region of time step dependent on the parameters in the new scheme is explicitly obtained. Finally, some examples of dynamic problems are given to show the accuracy and efficiency of the proposed scheme applied in dynamic systems.},
  doi            = {10.1007/s10409-006-0050-5},
  file           = {:paper\\A new simple method of implicit time integration for dynamic problems of engineering structures.pdf:PDF},
  groups         = {Dynamic equation numerical methods},
  keywords       = {qualityAssured},
  url            = {http://dx.doi.org/10.1007/s10409-006-0050-5},
}

@Article{Muǧan2001,
  author    = {Muǧan, Ata and Hulbert, Gregory M.},
  title     = {Frequency‐domain analysis of time‐integration methods for semidiscrete finite element equations—part I: Parabolic problems},
  journal   = {International Journal for Numerical Methods in Engineering},
  year      = {2001},
  volume    = {51},
  number    = {3},
  pages     = {333--350},
  month     = {5},
  issn      = {1097-0207},
  abstract  = {Time-integration methods for semidiscrete equations emanating from parabolic differential equations are analysed in the frequency domain. The discrete-time transfer functions of three popular methods are derived, and subsequently the forced response characteristics of single modes are studied in the frequency domain. To enable consistent comparison of the frequency responses of different algorithms, three characteristic numbers are identified. Frequency responses and L2-norms of the phase and magnitude errors are compared for the three time-integration algorithms. The examples demonstrate that frequency-domain analysis provides substantial insight into the time-domain properties of time-integration algorithms. Copyright © 2001 John Wiley & Sons, Ltd.},
  doi       = {10.1002/nme.134},
  file      = {:paper\\Control theory\\Frequency-domain analysis of time-integration methodsfor semidiscrete nite element equations—part I Parabolic problems.pdf:PDF},
  groups    = {Control theory},
  publisher = {Wiley Online Library},
  url       = {http:https://dx.doi.org/10.1002/nme.134},
}

@Article{yukaiping2004,
  author   = {于开平，赵婕，樊久铭，邹经湘},
  title    = {结构动力响应数值分析的新的广义-α方法的频率域分析},
  journal  = {振动工程学报},
  year     = {2004},
  volume   = {04},
  number   = {6},
  pages    = {21-26},
  language = {zh},
  abstract = {介绍了一族用于结构动力响应数值计算的新的广义-α方法。用这族算法将结构动力学方程离散得到关于位移的差分方程,然后做Z-变换得到算法传递函数、算法频率响应函数。通过算法离散的频率响应和系统真实频率响应的比较,在频率域内分析了这族算法的性能,并与常用的Newmark平均加速度方法进行了比较。在分析中,以激励力频率为自变量分析了算法的Bode图,以系统固有频率和激励力频率与时间步长的乘积为自变量在三维空间内分析了算法频率响应幅值绝对误差,又以算法自由参数、阻尼比为自变量在三维空间内分析了算法频率响应幅值2-范数意义下的相对误差。结果表明,新算法的性能得到了更深刻的认识。},
  file     = {:paper\\Control theory\\结构动力响应数值分析的新的广义_方法的频率域分析_于开平.pdf:PDF},
  groups   = {Control theory},
  keywords = {结构动力学;动力响应;时间积分算法;频率域分析;频率响应函数},
}

