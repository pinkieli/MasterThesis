@Article{Abdi2016,
  author   = {A. Abdi},
  title    = {Construction of high-order quadratically stable second-derivative general linear methods for the numerical integration of stiff \{ODEs\}},
  journal  = {Journal of Computational and Applied Mathematics},
  year     = {2016},
  volume   = {303},
  pages    = {218 - 228},
  issn     = {0377-0427},
  abstract = {Abstract Theory of general linear methods (GLMs) for the numerical solution of autonomous system of ordinary differential equations of the form y ′ = f ( y ) is extended to include the second derivative y ″ = g ( y ) : = f ′ ( y ) f ( y ) . This extension of \{GLMs\} is called second derivative general linear methods (SGLMs). In this paper we will construct two-stage A - and L -stable \{SGLMs\} of order p and stage order q = p up to six with low error constants. We will show the efficiency of the proposed methods by implementing on some well-known stiff problems. },
  doi      = {https://doi.org/10.1016/j.cam.2016.02.054},
  groups   = {J.C. Butcher, General linear method},
  keywords = {Stiff differential equations, General linear methods, Second derivative methods, A - and L - stability, Quadratic stability },
  url      = {http://www.sciencedirect.com/science/article/pii/S037704271630111X},
}

@Article{Butcher2017,
  author   = {John C. Butcher and Raffaele D'Ambrosio},
  title    = {Partitioned general linear methods for separable Hamiltonian problems},
  journal  = {Applied Numerical Mathematics},
  year     = {2017},
  volume   = {117},
  pages    = {69 - 86},
  issn     = {0168-9274},
  abstract = {Abstract Partitioned general linear methods possessing the G-symplecticity property are introduced. These are intended for the numerical solution of separable Hamiltonian problems and, as for multivalue methods in general, there is a potential for loss of accuracy because of parasitic solution growth. The solution of mechanical problems over extended time intervals often benefits from interchange symmetry as well as from symplectic behaviour. A special type of symmetry, known as interchange symmetry, is developed from a model Runge–Kutta case to a full multivalue case. Criteria are found for eliminating parasitic behaviour and order conditions are explored. },
  doi      = {https://doi.org/10.1016/j.apnum.2017.02.001},
  groups   = {J.C. Butcher, General linear method},
  keywords = {Separable Hamiltonian problems, Multivalue numerical methods, G-symplecticity, Symmetric methods, Parasitic components },
  url      = {http://www.sciencedirect.com/science/article/pii/S0168927417300375},
}

@Article{Horvath2016,
  author   = {Zoltán Horváth and Helmut Podhaisky and Rüdiger Weiner},
  title    = {Strong stability preserving explicit peer methods},
  journal  = {Journal of Computational and Applied Mathematics},
  year     = {2016},
  volume   = {296},
  pages    = {776 - 788},
  issn     = {0377-0427},
  abstract = {Abstract In this paper we study explicit peer methods up to order p = 13 which have the strong stability preserving (SSP) property. This class of general linear methods has the favourable property of a high stage order. The effective \{SSP\} coefficient is maximized by solving a nonlinear constraint optimization problem numerically to high precision. The coefficient matrices of the optimized methods are sparse in a very structured way. Linear multistep methods are obtained as a special case of only one stage. },
  doi      = {https://doi.org/10.1016/j.cam.2015.11.005},
  groups   = {J.C. Butcher, General linear method},
  keywords = {SSP, Positivity, Explicit peer methods, Nonstiff initial value problems },
  url      = {http://www.sciencedirect.com/science/article/pii/S0377042715005506},
}

