  type                       = {{Proceedings Paper}},
  unique-id                  = {{ISI:000282620500014}},
  usage-count-last-180-days  = {{0}},
  usage-count-since-2013     = {{0}},
  web-of-science-categories  = {{Engineering, Civil; Engineering, Mechanical}},
