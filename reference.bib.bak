@InProceedings{Chang2006a,
  author    = {Chang, S.Y. and Sung, Y.C.},
  title     = {An enhanced explicit pseudodynamic algorithm with unconditional stability},
  year      = {2006},
  volume    = {3},
  pages     = {1505 - 1514},
  address   = {San Francisco, CA, United states},
  note      = {Error propagation;Iteration procedure;Multiple-degree-of-freedom;Numerical properties;Pseudo-dynamic testing;Substructure systems;Time step;Unconditional stability;Unconditionally stable;},
  abstract  = {An explicit pseudodynamic algorithm is preferred over an implicit pseudodynamic algorithm since it involves no iteration procedure or extra hardware that is usually needed for an implicit pseudodynamic algorithm. However, its size of time step is often strictly constrained by stability. Therefore, it seems promising if an explicit pseudodynamic algorithm can have unconditional stability, which will eliminate the severe limitation on a time step for the pseudodynamic testing of a multiple degree of freedom system or a substructure system. Although an unconditionally stable explicit pseudodynamic algorithm (USEPA-1) has been developed by the first author an enhanced unconditionally stable explicit pseudodynamic algorithm (USEPA-2) is further proposed. It is interesting to find that both algorithms have exactly the same numerical properties. However, USEPA-2 possesses better error propagation properties than those of USEPA-1.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  file      = {:paper\\Unconditionally Stable Explicit Method\\An enhanced explicit pseudodynamic algorithm with unconditional stability.pdf:PDF},
  groups    = {Unconditionally Stable Explicit Method},
  journal   = {8th US National Conference on Earthquake Engineering 2006},
  key       = {Algorithms},
  keywords  = {Civil engineering;Engineering geology;},
  language  = {English},
}

@Article{Chang2009a,
  author    = {Chang, S.-Y.},
  title     = {Accurate integration of nonlinear systems using newmark explicit method},
  journal   = {Journal of Mechanics},
  year      = {2009},
  volume    = {25},
  number    = {3},
  pages     = {289 - 297},
  issn      = {17277191},
  note      = {Accuracy;Analytical evaluation;Degree of non-linearity;Explicit method;Instantaneous degree of nonlinearity;Linear elastic system;Numerical example;Numerical properties;Stability limit;Step-by-step;Time step;},
  language  = {English},
  abstract  = {In the step-by-step solution of a linear elastic system, an appropriate time step can be selected based on analytical evaluation results. However, there is no way to select an appropriate time step for accurate integration of a nonlinear system. In this study, numerical properties of the Newmark explicit method are analytically evaluated after introducing the instantaneous degree of nonlinearity. It is found that the upper stability limit is equal to 2 only for a linear elastic system. In general, it reduces for instantaneous stiffness hardening and it is enlarged for instantaneous stiffness softening. Furthermore, the absolute relative period error increases with the increase of instantaneous degree of nonlinearity for a given product of the natural frequency and the time step. The rough guidelines for accurate integration of a nonlinear system are also proposed in this paper based on the analytical evaluation results. Analytical evaluation results and the feasibility of the rough guidelines proposed for accurate integration of a nonlinear system are confirmed with numerical examples.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  file      = {:paper\\Unconditionally Stable Explicit Method\\Accurate integration of nonlinear systems using newmark explicit method.pdf:PDF},
  groups    = {Unconditionally Stable Explicit Method},
  key       = {System stability},
  keywords  = {Nonlinear systems;Stiffness;},
}

@Article{Chang2006b,
  author    = {Chang, S.-Y. and Huang, Y.-C. and Wang, C.-H.},
  title     = {Analysis of newmark explicit integration method for nonlinear systems},
  journal   = {Journal of Mechanics},
  year      = {2006},
  volume    = {22},
  number    = {4},
  pages     = {321 - 330},
  issn      = {17277191},
  note      = {Accuracy;Step degree of nonlinearity;Upper stability;},
  language  = {English},
  abstract  = {Numerical properties of the Newmark explicit method in the solution of nonlinear systems are explored. It is found that the upper stability limit is no longer equal to 2 for the Newmark explicit method for nonlinear systems. In fact, it is enlarged for stiffness softening and is reduced for stiffness hardening. Furthermore, its relative period error increases with the increase of the step degree of nonlinearity for a given value of the product of the natural frequency and the time step. It is also verified that the viscous damping determined from an initial stiffness is effective to reduce displacement response in the solution of a nonlinear system as that for solving a linear elastic system. All the theoretical results are confirmed with numerical examples.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  file      = {:paper\\Unconditionally Stable Explicit Method\\Analysis of Newmark Explicit Integration Method for Nonlinear Systems.pdf:PDF},
  groups    = {Unconditionally Stable Explicit Method},
  key       = {Nonlinear systems},
  keywords  = {Elasticity;Integration;Natural frequencies;Stiffness;Viscosity;},
}

