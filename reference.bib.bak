@Article{Popolizio2008,
  author    = {Popolizio, M. and Simoncini, V.},
  title     = {Acceleration techniques for approximating the matrix exponential operator},
  journal   = {SIAM Journal on Matrix Analysis and Applications},
  year      = {2008},
  volume    = {30},
  number    = {2},
  pages     = {657 - 683},
  issn      = {08954798},
  note      = {Acceleration parameters;Acceleration technique;Apriori;Krylov Subspace;Large matrices;Matrix exponential;Negative semi-definite;New results;Numerical approaches;Numerical experiments;Numerical results;Partial fraction expansion;Performance evaluation;Preconditioning techniques;Rational function;Rational functions;Spectral transformations;Subspace projection;},
  language  = {English},
  abstract  = {In this paper we investigate some well-established and more recent methods that aim at approximating the vector exp (A) v when A is a large symmetric negative semidefinite matrix, by efficiently combining subspace projections and spectral transformations. We show that some recently developed acceleration procedures may be restated as preconditioning techniques for the partial fraction expansion form of an approximating rational function. These new results allow us to devise a priori strategies to select the associated acceleration parameters; theoretical and numerical results are shown to justify these choices. Moreover, we provide a performance evaluation among several numerical approaches to approximate the action of the exponential of large matrices. Our numerical experiments provide a new, and in some cases, unexpected picture of the actual behavior of the discussed methods. &copy; 2008 Society for Industrial and Applied Mathematics.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {Matrix exponential},
  key       = {Iterative methods},
  keywords  = {Function evaluation;Numerical methods;},
  url       = {http://dx.doi.org/10.1137/060672856},
}

@Article{Druskin2008,
  author    = {Druskin, Vladimir},
  title     = {On monotonicity of the Lanczos approximation to the matrix exponential},
  journal   = {Linear Algebra and Its Applications},
  year      = {2008},
  volume    = {429},
  number    = {7},
  pages     = {1679 - 1683},
