  journal   = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
  key       = {Problem solving},
  keywords  = {Artificial intelligence;Computer generated holography;Curve fitting;Defects;Expert systems;Genetic algorithms;Interferometry;},
  language  = {English},
  url       = {http://dx.doi.org/10.1007/3-540-45049-1_13},
}

@InProceedings{Rizos1996,
  author    = {Rizos, D.C. and Karabalis, D.L.},
  title     = {Dynamic through-the soil interaction of adjacent surface or buried structures},
  year      = {1996},
