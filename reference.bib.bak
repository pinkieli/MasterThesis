@Article{Hirota2006,
  author    = {Hirota, Chiaki and Ozawa, Kazufumi},
  title     = {Numerical method of estimating the blow-up time and rate of the solution of ordinary differential equations-An application to the blow-up problems of partial differential equations},
  journal   = {Journal of Computational and Applied Mathematics},
