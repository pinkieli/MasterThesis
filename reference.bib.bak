@Article{Elliott1996,
  author    = {Elliott, P.J. and Diedrichsen, J. and Goodson, K.J. and Riste-Smith, R. and Sivewright, G.J.},
  title     = {Object-oriented system for 3D medical image analysis},
  journal   = {IBM Systems Journal},
  year      = {1996},
  volume    = {35},
  number    = {1},
  pages     = {4 - 24},
  issn      = {00188670},
  note      = {B spline surface patches;Three dimensional conformal radiation treatment;},
  language  = {English},
  abstract  = {As part of a European Commission-funded research project on medical image analysis, we have developed a system aimed at solving a real clinical problem: the outlining of the target volume and organs at risk for three-dimensional conformal radiation treatment planning. The clinical requirement is to make this process less tedious and time-consuming. We show how object-oriented design techniques can be used to good effect in a collaborative project such as this, so that image segmentation and other algorithms from a number of different partners in the consortium can be integrated into one system, for comparative evaluation by the clinical partners. We describe in some detail two of these algorithms, one a straightforward region and volume grower, in which particular attention has been paid to adapting the user interface to suit clinical needs, and the other an interactive tool using b-spline surface patches for direct three-dimensional segmentation. We report on the clinical feedback we have received, which indicates that the most technically advanced algorithms are not necessarily the most useful from a clinician's point of view.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Medical imaging},
  keywords  = {Algorithms;Computational methods;Image analysis;Image segmentation;Imaging systems;Object oriented programming;Radiotherapy;Three dimensional;User interfaces;},
}

@InProceedings{Ferrari1989,
  author    = {Ferrari, L.},
  title     = {Application of B-splines in digital filtering},
  year      = {1989},
  pages     = {79 -},
  address   = {Pacific Grove, CA, USA},
  note      = {Aliasing;Average Filters;B-Splines;FIR (Finite-Impulse-Response) Digital Filter;Prefilter;Summary Form Only;},
  abstract  = {Summary form only given, as follows. In many digital signal processing applications, the input signal rate is higher than the output signal Nyquist rate. In the case of an FIR (finite-impulse-response) digital filter, it has been found that a B-spline representation of the filter response leads to minimal time and circuit complexities. The number of filter coefficients required in this implementation is reduced and corresponds to the lower sampling rate of the filter function. The aliasing usually associated with this lower sampling rate is substantially reduced by processing the input data with a prefilter which can be interpreted as an interpolator. The prefilter is implemented using a cascade of running average filters which requires no multipliers. The B-spline approach is suited to VLSI implementation and leads to substantial reductions in circuit complexity.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Electric Filters, Digital},
  keywords  = {Integrated Circuits, VLSI;Signal Processing--Digital Techniques;},
  language  = {English},
  url       = {http://dx.doi.org/10.1109/MDSP.1989.97037},
}

@InProceedings{Calkins1989,
  author    = {Calkins, Dale E. and Theodoracatos, Vassilios E. and Aguilar, Glenn D. and Bryant, Dennis M.},
  title     = {Small craft hull form surface definition in a high-level computer graphics design environment},
  year      = {1989},
  volume    = {97},
  pages     = {85 - 113},
  address   = {New York, NY, USA},
  note      = {B-Spline;Hull Form Curves;},
  abstract  = {The purpose of this research has been to develop and evaluate procedures for the mathematical modeling of hull forms. This has been accomplished through the application and integration of high-level computer graphics technology into the design process. High-level computer graphics are characterized by the ability to manipulate a realistically rendered image of a three-dimensional object, with a colored and shaded surface, in real time. The hull forms studied are appropriate to hard chine and round bilge small craft. The first interactive graphics program has been developed for the definition of developable hull surfaces such as the hard chine hull. Control curves, including the keel chine and sheer lines, are faired interactively through real-time graphical techniques on the workstation using the Curve Fairing Tool (CFT) program. A developable surface is then generated between the control curves using the Developable surface Definition Tool (DSDT). B-spline mathematical techniques have been found to be particularly useful for defining complex three-dimensional sculpted surfaces, such as the round bilge hull. A second interactive graphics program, SPLINE, has been developed which facilitates the interactive definition of the B-spline control graph and real-time manipulation of the model image. Finally, a technique has been developed for in situ measurements of an existing hull form which might serve for example, as a 'parent' for further development using either DSDT or SPLINE. The procedure is based on the use of laser light striping techniques to illuminate surface contours, in conjunction with video image recording. A processing procedure then applies inverse homogeneous coordinate transformations to obtain the planar contour coordinates which are used to define the surface geometry. This research is being conducted at the Computer-Aided Design Technology (CADTECH) research laboratory in the Department of Mechanical Engineering at the University of Washington.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  issn      = {00811661},
  journal   = {Transactions - Society of Naval Architects and Marine Engineers},
  key       = {Ships},
  keywords  = {Computer Graphics;Mathematical Models;Naval Architecture--Computer Applications;},
  language  = {English},
}

@Article{Aristodemo1994,
  author    = {Aristodemo, M. and Turco, E.},
  title     = {Boundary element discretization of plane elasticity and plate bending problems},
  journal   = {International Journal for Numerical Methods in Engineering},
  year      = {1994},
  volume    = {37},
  number    = {6},
  pages     = {965 - 987},
  issn      = {00295981},
  note      = {B spline approximation;Discretization;Plate bending;},
  language  = {English},
  abstract  = {The paper deals with the discretization of the integral equations arising in the boundary formulation of plane elasticity and plate bending problems. Particular attention is paid to the efficiency of the interpolation used in approximating the boundary quantities and to the precision and computational convenience in evaluating the boundary integrals. The proposed discretization model is based on the use of a quadratic B-spline approximation to represent the boundary variables and on the results from the analytical integration to compute the boundary coefficients. The advantages are those of accuracy and the saving of computer time. Some numerical results allow an analysis of the performance of the model.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Integral equations},
  keywords  = {Bending (deformation);Boundary element method;Elasticity;Interpolation;Mathematical models;},
}

@InProceedings{Nicolai2001,
  author    = {Nicolai, Jurgen and Gonschorek, Karl H.},
  title     = {Application of spline-based expansion and weighting functions for an efficient analysis of thin-wire structures},
  year      = {2001},
  volume    = {1},
  pages     = {173 - 178},
  address   = {Montrealm, Que., Canada},
  note      = {Thin-wire antennas;},
  abstract  = {An analysis of thin-wire antennas and scatterers using quadratic spline-based expansion and weighting functions within the Method of Moments (MoM) is presented. So-called quadratic B-Splines are introduced as a generalisation of the well-known rectangular and triangle expansion functions. The thin-wire version of the electric-field integral equation (EFIE) is solved by the Method of Moments (MoM) using Galerkin's method. The described procedure results in a symmetrical structure of the impedance matrix, which allows the application of symmetric solvers with the benefit of a very effective solving procedure. Due to the choice of the expansion and weighting functions the results are very precise for near-fields and points near the structure. It become possible to investigate structures with very small separations between the wires. Numerical examples demonstrate the suitability of the method for the EMC-analysis and show the advantages of a reduced solving time in the analysis of large systems and a lower limit for the shortest separation between two wires.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  issn      = {01901494},
  journal   = {IEEE International Symposium on Electromagnetic Compatibility},
  key       = {Electromagnetic compatibility},
  keywords  = {Approximation theory;Electric lines;Galerkin methods;Matrix algebra;Method of moments;Problem solving;Resistors;Splines;},
  language  = {English},
  url       = {http://dx.doi.org/10.1109/ISEMC.2001.950583},
}

@InProceedings{Chen1997,
  author    = {Chen, Liang-Chia and Lin, Grier C. I.},
  title     = {Reverse engineering physical models employing a sensor integration between 3D stereo detection and contact digitisation},
  year      = {1997},
  volume    = {3204},
  pages     = {146 - 155},
  address   = {Pittsburgh, PA, United states},
  note      = {Adaptive surfaces;Automatic digitisation;B splines;Coordinate measurement machines;Data filtering;Delaunay;Innovative approaches;Manufacturing processes;Physical models;Processing times;Reverse engineering processes;Sensor integrations;Single processes;},
  abstract  = {A vision-driven automatic digitisation process for free-form surface reconstruction has been developed, with a coordinate measurement machine (CMM) equipped with a touch-triggered probe and a CCD camera, in reverse engineering physical models. The process integrates 3D stereo detection, data filtering, Delaunay Triangulation, adaptive surface digitisation into a single process of surface reconstruction. By using mis innovative approach, surface reconstruction can be implemented automatically and accurately. Leastsquares B-spline surface models with the controlled accuracy of digitisation can be generated for further application in product design and manufacturing processes. One industrial application indicates mat this approach is feasible, and the processing time required in reverse engineering process can be significantly reduced up to more than 85%.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  issn      = {0277786X},
  journal   = {Proceedings of SPIE - The International Society for Optical Engineering},
  key       = {Surface reconstruction},
  keywords  = {Administrative data processing;Cameras;Charge coupled devices;Coordinate measuring machines;Engineering;Imaging systems;Industrial applications;Models;Product design;Reengineering;Repair;Restoration;Reverse engineering;Splines;Stereo vision;Surface chemistry;Three dimensional;},
  language  = {English},
  url       = {http://dx.doi.org/10.1117/12.294453},
}

@Article{Perez1994,
  author    = {Perez, J. and Catedra, M.F.},
  title     = {Application of physical optics to the RCS computation of bodies modeled with NURBS surfaces},
  journal   = {IEEE Transactions on Antennas and Propagation},
  year      = {1994},
  volume    = {42},
  number    = {10},
  pages     = {1404 - 1411},
  issn      = {0018926X},
  note      = {Bezeir patches;Computer aided geometric design;Cox-De Boor transform algorithms;Nonuniform rational B spline surfaces;},
  language  = {English},
  abstract  = {This paper presents a method for the computation of the monostatic radar cross section (RCS) of electrically large conducting objects modeled by nonuniform rational B-spline (NURBS) surfaces using physical optic (PO) technique. The NURBS surfaces are expanded in terms of rational Bezier patches by applying the Cox-De Boor transform algorithm. This transformation is justified because Bezier patches are numerically more stable than NURBS surfaces. The PO integral is evaluated over the parametric space of the Bezier surfaces using asymptotic integration. The scattering field contribution of each Bezier patch is expressed in terms of its geometric parameters. Excellent agreement with PO predictions is obtained. The method is quite efficient because it makes use of a small number of patches to model complex bodies, so it requires very little memory and computing time.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Radar cross section},
  keywords  = {Aircraft models;Algorithms;Bodies of revolution;Computer aided design;Convergence of numerical methods;Electromagnetic wave scattering;Mathematical models;Mathematical transformations;Optics;Surfaces;},
  url       = {http://dx.doi.org/10.1109/8.320747},
}

@Article{1998,
  author    = {de, Figueiredo, Rui J.P.},
  title     = {Generalized spline framework for mixed mode signal processing},
  journal   = {Proceedings of the IEEE International Conference on Electronics, Circuits, and Systems},
  year      = {1998},
  volume    = {2},
  pages     = {399 - 400},
  note      = {Mixed mode signal processing;Spline methods;Analog filters;},
  language  = {English},
  abstract  = {In the context of mixed-mode signal processing applications, a system-level framework is presented for best approximation of `analog (continuous-time) functions' by `digital (discrete-time) functions' based on dynamic source models. Specifically, let x(t), a less than or equal t less than or equal b, denote the input to an analog filter F, and y(t), a less than or equal t less than or equal b the corresponding output of F. We assume that the input x belongs to a class C of signals generated by a linear dynamical system, characterized by an m<sup>th</sup> order linear differential operator L(D), D = d/dt, driven by inputs of bounded energy. On this basis, we obtain the best discrete-time approximation F* to the analog filter F by interpolating the samples x(t<sub>1</sub>),...,x(t<sub>n</sub>) of x(t) by a generalized L-spline S(t) (corresponding to the differential operator L(D)), and then convolving S(t) with the impulse response of the analog filter F. This provides the optimal mapping F* (in min-max error sense) of the samples x(t<sub>1</sub>),...,x(t<sub>n</sub>) into the best approximation y*(t<sub>1</sub>),...,y*(t<sub>n</sub>) of the samples y(t<sub>1</sub>),...,y(t<sub>n</sub>) of F.},
  address   = {Lisboa, Portugal},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Signal filtering and prediction},
  keywords  = {Electric filters;Mathematical models;Mathematical operators;Differential equations;Approximation theory;Integration;Convolution;Conformal mapping;Interpolation;},
  url       = {http://dx.doi.org/10.1109/ICECS.1998.814908},
}

@Article{Kulkarni2000,
  author    = {Kulkarni, Deepak and Subbarayan, Ganesh},
  title     = {Dynamic model for predicting the motion of solder droplets during assembly},
  journal   = {Thermomechanical Phenomena in Electronic Systems -Proceedings of the Intersociety Conference},
  year      = {2000},
  volume    = {1},
  pages     = {368 - 376},
  note      = {Nonuniform rational B-spline;Solder droplets;},
  language  = {English},
  abstract  = {The dynamic motion of a solder droplet during assembly is a complex, unsteady, free surface problem involving surface tension and viscous effects. The motion of the droplet is coupled with the motion of the component or chip to be assembled and involves dynamic contact lines. A methodology based on a Non-Uniform Rational B-Spline (NURBS) discretization has been developed for the dynamic analysis of the droplet motion. A surface energy based formulation has been developed to incorporate the surface tension effects. The developed methodology leads to an updated Lagrangian scheme with a Galerkin in space and Least square in time formulation. The Non-Uniform Rational B-Spline (NURBS) representation used for the spatial discretization enables the method to handle problems involving complex droplet geometries. The ability of the NURBS representation to provide both global and local control, along with the least square method used in this methodology, enables us to develop an unconditionally stable time integration scheme which can be optimized to achieve desired accuracy and numerical dissipation efficiently. A sample problem of droplet shape evolution has been solved to demonstrate the path prediction capability of the proposed methodology. In future, the method can be applied to solve various real world dynamic motion problems associated with droplets.},
  address   = {Las Vegas, NV, USA},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Surface mount technology},
  keywords  = {Galerkin methods;Lagrange multipliers;Least squares approximations;Navier Stokes equations;Surface tension;},
}

@Article{Panton1975,
  author    = {Panton, Ronald L. and Sallee, H.Bruce},
  title     = {SPLINE FUNCTION REPRESENTATIONS FOR COMPUTER SOLUTIONS TO FLUID PROBLEMS.},
  journal   = {Computers and Fluids},
  year      = {1975},
  volume    = {3},
  number    = {2-3},
  pages     = {257 - 269},
  issn      = {00457930},
  note      = {COLLOCATION METHOD;SPLINE FUNCTIONS;},
  abstract  = {Spline functions are used in the computation of several heat conduction and boundary layer problems. Three types of spline representations were investigated using both collation and integral methods of solution. The difficulties to be expected in problem formulation, computer programming, ill-conditioning of the matrices, and computational times and accuracies are discussed for the various representations and methods. The B-spline basis was concluded to be superior when used in collocation methods. Splines are not competitive with finite-difference methods for many problems, but will be advantageous in situations where computer storage must be minimized. A discussion of two-dimensional spline representations is included.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {HEAT TRANSFER},
  keywords  = {COMPUTER AIDED DESIGN;FLOW OF FLUIDS - Boundary Layer;MATHEMATICAL TECHNIQUES - Approximation Theory;},
}

@Article{Namadchi2016,
  author     = {Namadchi, Amir Hossein and Alamatian, Javad},
  title      = {Explicit dynamic analysis using Dynamic Relaxation method},
  journal    = {Computers \& Structures},
  year       = {2016},
  volume     = {175},
  pages      = {91--99},
  month      = oct,
  abstract   = {This paper deals with a new explicit framework for numerical dynamic analysis that could be applied to any explicit-implicit time integration, i.e. predictor-corrector time integrations. For this purpose, Dynamic Relaxation method is combined with the explicit-implicit time integrations so that the proposed algorithm is performed by full vector operations. This subject helps to obtain an explicit algorithm, which does not involve solving simultaneous system of equations in each time increment, even linear one. Moreover, the number of corrections, which is the main concern in the explicit-implicit algorithms, is selected automatically in the suggested procedure. Afterwards, some numerical examples are solved to illustrate the validity and the robustness of the proposed algorithm. Results show that applying the proposed framework to the higher order explicit-implicit integrations exhibits excellent accuracy in comparison with the well-known implicit Newmark-beta and Wilson-theta schemes. Simplicity, more stable integration, automated procedure and vector operations are some of the salient features of the proposed algorithm. (C) 2016 Elsevier Ltd. All rights reserved.},
  doi        = {10.1016/j.compstruc.2016.07.008},
