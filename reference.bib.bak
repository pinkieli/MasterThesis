  title      = {Unconditionally stable subinterval method of displacement element based on cubic B-spline},
  journal    = {Tianjin Daxue Xuebao (Ziran Kexue yu Gongcheng Jishu Ban)/Journal of Tianjin University Science and Technology},
  year       = {2007},
  volume     = {40},
  number     = {10},
