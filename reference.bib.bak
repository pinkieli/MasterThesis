  journal-iso                = {{J. Earthqu. Eng.}},
  keywords                   = {{instantaneous degree of nonlinearity; stability; accuracy}},
  keywords-plus              = {{STRUCTURAL DYNAMICS; PSEUDODYNAMIC ALGORITHM; INTEGRATION; DISSIPATION}},
  number-of-cited-references = {{21}},
