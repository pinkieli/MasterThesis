@Article{Bin2013,
  author    = {Bin, Jonghoon and Oates, William S. and Yousuff Hussaini, M.},
  title     = {Fluid-structure interactions of photo-responsive polymer cantilevers},
  journal   = {Journal of Fluids and Structures},
  year      = {2013},
  volume    = {37},
  pages     = {34 - 61},
  issn      = {08899746},
  note      = {Adaptive structure;Ambient pressures;Arbitrary Lagrangian Eulerian;Arbitrary Lagrangian Eulerian method;Artificial compressibility;Bending deformations;Cantilever plate;Central scheme;Cubic B -spline;Curvilinear coordinate systems;Fast response;Finite-volume formulation;Fluid medium;Incompressible Navier Stokes equations;Interface boundaries;Liquid crystal networks;Material behavior;Numerical approximations;Numerical example;Photo-responsive polymer;Second orders;Shape functions;Solid-fluid interfaces;Time integration;},
  language  = {English},
  abstract  = {A new class of photomechanical liquid crystal networks (LCNs) has emerged, which generate large bending deformation and fast response times that scale with the resonance of the polymer films. Here, a numerical study is presented that describes the photomechanical structural dynamic behavior of an LCN in a fluid medium; however, the methodology is also applicable to fluid-structure interactions of a broader range of adaptive structures. Here, we simulate the oscillation of photomechanical cantilevers excited by light while simultaneously modeling the effect of the surrounding fluid at different ambient pressures. The photoactuated LCN is modeled as an elastic thin cantilever plate, and gradients in photostrain from the external light are computed from the assumptions of light absorption and photoisomerization through the film thickness. Numerical approximations of the equations governing the plate are based on cubic B-spline shape functions and a second order implicit Newmark central scheme for time integration. For the fluid, three dimensional unsteady incompressible Navier-Stokes equations are solved using the arbitrary Lagrangian-Eulerian (ALE) method, which employs a structured body-fitted curvilinear coordinate system where the solid-fluid interface is a mesh line of the system, and the complicated interface boundary conditions are accommodated in a conventional finite-volume formulation. Numerical examples are given which provide new insight into material behavior in a fluid medium as a function of ambient pressure. &copy; 2012.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Fluid structure interaction},
  keywords  = {Bending (deformation);Lagrange multipliers;Liquid crystals;Nanocantilevers;Navier Stokes equations;Plates (structural components);Polymer films;Structural dynamics;},
  url       = {http://dx.doi.org/10.1016/j.jfluidstructs.2012.10.008},
}

@Article{Lin2011,
  author    = {Lin, Zhengyu and Wang, Jiabin and Howe, David},
  title     = {A learning feed-forward current controller for linear reciprocating vapor compressors},
  journal   = {IEEE Transactions on Industrial Electronics},
  year      = {2011},
  volume    = {58},
  number    = {8},
  pages     = {3383 - 3390},
  issn      = {02780046},
  note      = {B-spline neural network;Current controller;Current tracking errors;Direct drive;Effective solution;Extensive simulations;Feed-Forward;Highly nonlinear;Learning control systems;Linear compressors;Low costs;Motor currents;Proportional integral controllers;Real time;Reliable operation;Sinusoidal currents;Steady state;Transient performance;Vapor compressors;},
  language  = {English},
  abstract  = {Direct-drive linear reciprocating compressors offer numerous advantages over conventional counterparts which are usually driven by a rotary induction motor via a crank shaft. However, to ensure efficient and reliable operation under all conditions, it is essential that motor current of a linear compressor follows a sinusoidal current command with a frequency which matches the system resonant frequency. The design of a high-performance current controller for linear compressor drive presents a challenge since the system is highly nonlinear, and an effective solution must be low cost. In this paper, a learning feed-forward current controller for the linear compressors is proposed. It comprises a conventional feedback proportional-integral controller and a feed-forward B-spline neural network (BSNN). The feed-forward BSNN is trained online and in real time in order to minimize the current tracking error. Extensive simulation and experiment results with a prototype linear compressor show that the proposed current controller exhibits high steady state and transient performance. &copy; 2009 IEEE.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Controllers},
  keywords  = {Compressors;Electric current control;Feedback;Induction motors;Natural frequencies;Neural networks;Power quality;Reciprocating compressors;Splines;},
  url       = {http://dx.doi.org/10.1109/TIE.2010.2089948},
}

@Article{DeCraene2012,
  author    = {De Craene, Mathieu and Piella, Gemma and Camara, Oscar and Duchateau, Nicolas and Silva, Etelvino and Doltra, Adelina and D'hooge, Jan and Brugada, Josep and Sitges, Marta and Frangi, Alejandro F.},
  title     = {Temporal diffeomorphic free-form deformation: Application to motion and strain estimation from 3D echocardiography},
  journal   = {Medical Image Analysis},
  year      = {2012},
  volume    = {16},
  number    = {2},
  pages     = {427 - 450},
  issn      = {13618415},
  note      = {3-D ultrasound;3D echocardiography;B-spline;Cardiac resynchronization;Clinical outcome;Data sets;Diffeomorphisms;Displacement field;End-systolic;Energy functionals;Eulerian;FFD;Free-form deformation;Image similarity;Left ventricles;Left ventricular;Longitudinal strain;Myocardial tissue;Noise levels;Nonstationary;Optimal setting;Registration algorithms;Spatial derivatives;Spatio-temporal registration;State-of-the-art algorithms;Strain estimation;Strain patterns;Strain tensor;Sum of squared differences;Synthetic sequence;Temporal resolution;Time consistency;Velocity field;},
  language  = {English},
  abstract  = {This paper presents a new registration algorithm, called Temporal Diffeomorphic Free Form Deformation (TDFFD), and its application to motion and strain quantification from a sequence of 3D ultrasound (US) images. The originality of our approach resides in enforcing time consistency by representing the 4D velocity field as the sum of continuous spatiotemporal B-Spline kernels. The spatiotemporal displacement field is then recovered through forward Eulerian integration of the non-stationary velocity field. The strain tensor is computed locally using the spatial derivatives of the reconstructed displacement field. The energy functional considered in this paper weighs two terms: the image similarity and a regularization term. The image similarity metric is the sum of squared differences between the intensities of each frame and a reference one. Any frame in the sequence can be chosen as reference. The regularization term is based on the incompressibility of myocardial tissue. TDFFD was compared to pairwise 3D FFD and 3D+t FFD, both on displacement and velocity fields, on a set of synthetic 3D US images with different noise levels. TDFFD showed increased robustness to noise compared to these two state-of-the-art algorithms. TDFFD also proved to be more resistant to a reduced temporal resolution when decimating this synthetic sequence. Finally, this synthetic dataset was used to determine optimal settings of the TDFFD algorithm. Subsequently, TDFFD was applied to a database of cardiac 3D US images of the left ventricle acquired from 9 healthy volunteers and 13 patients treated by Cardiac Resynchronization Therapy (CRT). On healthy cases, uniform strain patterns were observed over all myocardial segments, as physiologically expected. On all CRT patients, the improvement in synchrony of regional longitudinal strain correlated with CRT clinical outcome as quantified by the reduction of end-systolic left ventricular volume at follow-up (6 and 12. months), showing the potential of the proposed algorithm for the assessment of CRT. &copy; 2011 Elsevier B.V.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Three dimensional},
  keywords  = {Algorithms;Blood pressure;Deformation;Echocardiography;Image analysis;Patient monitoring;Patient treatment;Strain;Tissue;Ultrasonics;Velocity;},
  url       = {http://dx.doi.org/10.1016/j.media.2011.10.006},
}

@Article{Shao2011,
  author    = {Shao, Jingfeng and Li, Yonggang},
  title     = {Multi-agent production monitoring and management system for textile materials and its applications},
  journal   = {Journal of Industrial Textiles},
  year      = {2011},
  volume    = {40},
  number    = {4},
  pages     = {380 - 399},
  issn      = {15280837},
  note      = {B spline functions;Central point;Comprehensive information;Data fittings;Data Sharing;Evaluated data;Information integration;Management level;Material data;Monitoring system;Multi-Agent;Multi-agent monitoring;Production monitoring;Production process;Real time monitoring;Scientific management;textile materials;},
  language  = {English},
  abstract  = {To improve management level of the textile factory, achieve real-time monitoring of production process of textile materials, and realize on-line management of the comprehensive information as well as to solve the problem of data isolation within the local area network, a new multi-agent production monitoring and management system for textile materials was developed. In the article, first, we analyzed the actual requirements of the textile factory, researched the weaknesses in the used monitoring systems, and proposed a production monitoring and management system model based on multi-agent. Then, we made the system collect the data in a central point, divided data into two parts (noise and use-full information), and analyzed the correlation of material data. Third, we compared the results of several data fitting, and finally decided to use B-spline function to optimize the material data. Finally, using agent technique, expert system and fuzzy control theory, we evaluated data results of the textile material. As verified by application, on being compared with the used monitoring systems, multi-agent monitoring and management system can objectively reflect production process of textile materials, achieve comprehensive management and information integration, and realize data sharing and scientific management for all of the workshops. &copy; The Author(s), 2010.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Monitoring},
  keywords  = {Expert systems;Fuzzy control;Information management;Local area networks;Multi agent systems;Production engineering;Splines;Textile industry;Textile processing;Textiles;},
  url       = {http://dx.doi.org/10.1177/1528083710380428},
}

@Article{Chen2003,
  author    = {Chen, Meng and Chen, Liping and Zhang, Xinfang and Bai, Hemin and Xiao, Yuzhi},
  title     = {Research and dynamic simulation of docking locks with contact-impact},
  journal   = {Aerospace Science and Technology},
