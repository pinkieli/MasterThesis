@Article{Noh2013,
  author           = {Noh, Gunwoo and Bathe, Klaus-Juergen},
  title            = {An explicit time integration scheme for the analysis of wave propagations},
  journal          = {Computers \& Structures},
  year             = {2013},
  volume           = {129},
  pages            = {178-193},
  month            = dec,
  issn             = {0045-7949},
  abstract         = {A new explicit time integration scheme is presented for the solution of
   wave propagation problems. The method is designed to have small solution
   errors in the frequency range that can spatially be represented and to
   cut out high spurious frequencies. The proposed explicit scheme is
