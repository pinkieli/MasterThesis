@Article{Givoli1993,
  author    = {Givoli, Dan and Henigsberg, Ilan},
  title     = {A simple time-step control scheme},
  journal   = {Communications in Numerical Methods in Engineering},
  year      = {1993},
  volume    = {9},
  number    = {11},
  pages     = {873--881},
