  address   = {Virginia Beach, VA, United states},
  note      = {adaptive;Autonomous underwater vehicles (AUVs);B spline curve;B-spline;B-spline approximation;Computation efficiency;Computation time;Efficient path;Fitness evaluations;Fixed numbers;Internal nodes;Optimal trajectories;Path planners;Position estimates;Smooth paths;Uncertain environments;uncertainty;},
  abstract  = {This paper presents an optimal and efficient path planner using adaptive B-spline approximation mechanism for Autonomous Underwater Vehicles (AUVs) operating in turbulent, cluttered and uncertain environments. The proposed method recursively inserts midpoint knots until an approximated B-Spline curve that satisfies the accuracy criteria is achieved. The method is able to adapt the quantity of internal knots inserted based on the specific needs of each path to conform to its respective desired smooth path and satisfy the accuracy criteria. Consequently, this method effectively minimizes the number of internal knots for any given trajectory, thus effectively improving the computation efficiency of the path fitness evaluation and hence path planning. The proposed method is integrated with a Genetic Algorithm (GA) based path planner and tested to generate an optimal trajectory for an AUV travelling through a turbulent ocean field in scenarios with uncertainty in position estimates. Simulation results show that the resulting approach is able to quickly and effectively guide the AUV to its destination with significant savings in computation time compared with B-Spline based planners with fixed numbers of internal nodes. &copy; 2012 IEEE.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  journal   = {OCEANS 2012 MTS/IEEE: Harnessing the Power of the Ocean},
  key       = {Autonomous underwater vehicles},
  keywords  = {Curve fitting;Genetic algorithms;Interpolation;Motion planning;Oceanography;Optimization;Uncertainty analysis;},
  language  = {English},
  url       = {http://dx.doi.org/10.1109/OCEANS.2012.6405066},
}

@Article{Mohammadi2013,
  author    = {Mohammadi, Reza},
  title     = {B-spline collocation algorithm for numerical solution of the generalized Burger's-Huxley equation},
  journal   = {Numerical Methods for Partial Differential Equations},
  year      = {2013},
  volume    = {29},
