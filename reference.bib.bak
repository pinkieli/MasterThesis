@Article{Schrefler1998,
  author    = {Schrefler, BA and Zhang, HW and Pastor, M and Zienkiewicz, OC},
  title     = {{Strain localisation modelling and pore pressure in saturated sand samples}},
  journal   = {{COMPUTATIONAL MECHANICS}},
  year      = {{1998}},
  volume    = {{22}},
  number    = {{3}},
  pages     = {{266-280}},
  month     = {{SEP}},
  issn      = {{0178-7675}},
  abstract  = {{Dynamic strain localisation theory together with a multiphase material
   model is used to simulate shear band dominated processes in fully
   saturated sand samples. The fluid-saturated medium is viewed as
   multi-phase continuum consisting of a solid skeleton and pores filled by
   fluids. The governing equations are based on the general framework of
   averaging theories. A generalised plasticity constitutive model for
   fully saturated soils is adopted in the computational process. Both
   samples of medium-loose and dense sands are studied. Negative water
