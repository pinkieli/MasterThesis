  keywords  = {Boundary conditions;Linear systems;Mathematical operators;Number theory;Numerical methods;Ordinary differential equations;},
  owner     = {Pinkie.li},
  timestamp = {2017-09-14},
  url       = {http://dx.doi.org/10.1137/120865458},
}

@Article{Reyes2016,
