@Article{Nobari2010,
  author    = {Nobari, Elham and Mohammad Hosseini, S.},
  title     = {A method for approximation of the exponential map in semidirect product of matrix Lie groups and some applications},
  journal   = {Journal of Computational and Applied Mathematics},
  year      = {2010},
  volume    = {234},
  number    = {1},
  pages     = {305 - 315},
  issn      = {03770427},
  note      = {Campbell;Generalized polar decomposition;Hausdorff;Lie group;Semidirect product;},
  language  = {English},
  abstract  = {In this paper we explore the computation of the matrix exponential in a manner that is consistent with Lie group structure. Our point of departure is the decomposition of Lie algebra as the semidirect product of two Lie subspaces and an application of the Baker-Campbell-Hausdorff formula. Our results extend the results in Iserles and Zanna (2005)&nbsp;[2], Zanna and Munthe-Kaas(2001/02)&nbsp;[4] to a range of Lie groups: the Lie group of all solid motions in Euclidean space, the Lorentz Lie group of all solid motions in Minkowski space and the group of all invertible (upper) triangular matrices. In our method, the matrix exponential group can be computed by a less computational cost and is more accurate than the current methods. In addition, by this method the approximated matrix exponential belongs to the corresponding Lie group. &copy; 2010 Elsevier B.V. All rights reserved.},
