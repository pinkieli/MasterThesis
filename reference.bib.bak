@Article{SAVCHENKO1992,
  author                     = {SAVCHENKO, VS},
  title                      = {{THE CONSTRUCTION OF SETS OF ATTAINABILITY IN A CLASS OF NONLINEAR CONTROL-SYSTEMS}},
  journal                    = {{JOURNAL OF COMPUTER AND SYSTEMS SCIENCES INTERNATIONAL}},
  year                       = {{1992}},
  volume                     = {{30}},
  number                     = {{6}},
  pages                      = {{146-150}},
  month                      = {{NOV-DEC}},
  issn                       = {{1064-2307}},
  abstract                   = {{Examines a class of dynamic objects described by a system of ordinary
   differential equations. The equations are linear in the control but
   nonlinear in the phase variables. The range of control depends on the
   phase variables. A method is proposed for constructing internal
   estimates of the set of attainability after a time T that depend on a
   numerical parameter R. It is shown that, in the limit as R --> infinity,
   these estimates coincide with the boundary of the set of attainability.
   Examples are included.}},
