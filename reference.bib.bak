@InProceedings{Hughes2006a,
  author               = {Hughes, T. J. R. and Cottrell, J. A. and Bazilevs, Y. and Reali, A.},
  title                = {{Computational geometry as a basis for computational structures technology: A look into the future}},
  booktitle            = {{Innovation in Computational Structures Technology}},
  year                 = {{2006}},
  editor               = {{Topping, BHV and Montero, G and Montenegro, R}},
  pages                = {{1-22}},
  organization         = {{Univ Las Palmas Gran Canaria; IUSIANI; SEMNI; SeMA; Gobierno Canarias; Minist Educ Ciencia; Computat Technol Solut; Comp \& Structures; FIRGAS; Univ Las Palmas Gran Canaria, Dept Math; GranCanaria Convent Bur}},
  note                 = {{8th International Conference on Computational Structures Technology, Las Palmas, SPAIN, SEP 12-15, 2006}},
  abstract             = {{In anticipation of the future in which Computational Geometry will play
   a more prominent role in Computational Structures Technology, we present
   an introduction to basic concepts of Isogeometric Analysis. NURBS
   (non-uniform rational B-Splines) are employed to instantiate the
   Isogeometric Analysis concept. These enable high-precision geometry
   modelling and vastly simplify refinement strategies. NURBS are a
   standard CAD technology and have the potential for better integrating
   analysis with geometry. Applications to structural vibration problems
   indicate NURBS are much better behaved than standard finite elements.}},
  groups               = {Hughes T.J.R.},
  isbn                 = {{978-1-874672-27-2}},
  orcid-numbers        = {{Reali, Alessandro/0000-0002-0639-7067}},
  researcherid-numbers = {{Reali, Alessandro/D-5065-2011}},
  unique-id            = {{ISI:000248945600001}},
}

@InProceedings{Hughes2002,
  author    = {Hughes, TJR and Oberai, AA},
  title     = {{The variational multiscale formulation of LES with application to turbulent channel flows}},
  booktitle = {{GEOMETRY, MECHANICS AND DYNAMICS: VOLUME IN HONOR OF THE 60TH BIRTHDAY OF J. E. MARSDEN}},
  year      = {{2002}},
  editor    = {{Newton, P and Holmes, P and Weinstein, A}},
  pages     = {{223-239}},
  note      = {{Workshop on Geometry, Dynamics and Mechanics in Honour of the 60th Birthday of Jerrold E Marsden, FIELD INST RES MATH SCI, TORONTO, CANADA, AUG 07-10, 2002}},
  abstract  = {{We begin by recalling old times when the senior author and Jerry Marsden
   collaborated on research in mechanics in the 1970's. We note that both
   our recent interests have been focused on turbulence, although with
   different approaches. The common theme is reliance on variational
   structure. We then get down to business and describe our approach-the
   variational multiscale formulation of LES. Application is made to
   turbulent two-dimensional equilibrium and three-dimensional
   non-equilibrium Channel flows. Simple, constant-coefficient
   Smagorinsky-type eddy viscosities, without wall damping functions, are
   used to model the transfer of energy from small resolved scales to
   unresolved scales, an approach which is not viable within the
   traditional LES framework. Nevertheless, very good results are obtained.}},
  doi       = {{10.1007/0-387-21791-6\_7}},
  groups    = {Hughes T.J.R.},
  isbn      = {{0-387-95518-6}},
  unique-id = {{ISI:000177687800007}},
}

