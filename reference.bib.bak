@Article{Huang1998,
  author    = {Huang, Maosong and Zienkiewicz, O. C.},
  title     = {New unconditionally stable staggered solution procedures for coupled soil-pore fluid dynamic problems},
  journal   = {International Journal for Numerical Methods in Engineering},
  year      = {1998},
  volume    = {43},
  number    = {6},
  pages     = {1029--1052},
  issn      = {1097-0207},
  abstract  = {A new class of unconditionally stable staggered implicit–implicit time-stepping algorithms for coupled soil-pore fluid dynamic problems is presented. The proposed schemes are stabilized with a pressure correction method and the staggered procedure introduced earlier by the second author is a simplified case of the new algorithms. The stabilized procedure is efficient in numerical time domain analysis of soil-pore fluid interaction occurring in earthquake and consolidation problems. Several examples of linear and non-linear problems demonstrate the stability and accuracy of the proposed numerical solution algorithms. © 1998 John Wiley & Sons, Ltd.},
  doi       = {10.1002/(SICI)1097-0207(19981130)43:6<1029::AID-NME459>3.0.CO;2-H},
  groups    = {W.L.WOOD},
