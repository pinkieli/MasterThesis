@Article{Bursi2010,
  author                     = {Bursi, Oreste S. and He, Leqia and Lamarche, Charles-Philippe and Bonelli, Alessio},
  title                      = {{Linearly Implicit Time Integration Methods for Real-Time Dynamic Substructure Testing}},
  journal                    = {{JOURNAL OF ENGINEERING MECHANICS-ASCE}},
  year                       = {{2010}},
  volume                     = {{136}},
  number                     = {{11}},
  pages                      = {{1380-1389}},
  month                      = {{NOV}},
  issn                       = {{0733-9399}},
  language                   = {{English}},
  abstract                   = {{The simulation in real time of heterogeneous systems has to guarantee
   that the time integration of the equations of motion is always
   successfully completed within an a priori fixed sampling time interval.
   Therefore, numerical and/or physical substructures as well as numerical
   solution methods have to be adapted to the needs of real-time
   simulations. Monolithic stable numerical methods are implicit and cannot
   be easily used in real-time applications because of their iterative
   strategies necessary to solve the nonlinear corrector equations. As an
   alternative, in the present paper, we consider linearly implicit
   Rosenbrock-based L-stable real-time (LSRT) compatible algorithms with
   both two-stage and three-stage. Moreover, other linearly implicit
   structural integrators used nowadays to perform coupled simulations in
   real time are introduced too. Successively, typical properties of
   monolithic algorithms are shown when large time steps are employed. The
   loss of stability and the reduction of accuracy of these algorithms,
   when applied to coupled systems caused by kinematically closed loops,
   are analyzed in-depth through a split-inertia substructured system. In
   this respect, the benefits of the L-stability property are shown.
   Finally, the performance of the algorithms under investigation appears
   in a number of more realistic tests considering both nonstiff and stiff
   substructures.}},
  address                    = {{1801 ALEXANDER BELL DR, RESTON, VA 20191-4400 USA}},
  affiliation                = {{Bursi, OS (Reprint Author), Univ Trent, Dipartimento Ingn Meccan \& Strutturale, Via Mesiano 77, I-38050 Trento, Italy. Bursi, Oreste S.; He, Leqia; Bonelli, Alessio, Univ Trent, Dipartimento Ingn Meccan \& Strutturale, I-38050 Trento, Italy. Lamarche, Charles-Philippe, Ecole Polytech, Dept Genies Civil Geol \& Mines, Montreal, PQ H3C 3A7, Canada.}},
  author-email               = {{Oreste.Bursi@ing.unitn.it}},
  doc-delivery-number        = {{667MF}},
  doi                        = {{10.1061/(ASCE)EM.1943-7889.0000182}},
  groups                     = {Dynamic equation numerical methods},
  journal-iso                = {{J. Eng. Mech.-ASCE}},
  keywords                   = {{Rosenbrock-based methods; Linearly implicit algorithm; Real-time substructure testing; Kinematically closed loops}},
  keywords-plus              = {{GENERALIZED-ALPHA METHODS; STRUCTURAL DYNAMICS; ROSENBROCK METHODS; MULTIBODY SYSTEMS; ALGORITHM; SIMULATION}},
  number-of-cited-references = {{37}},
  orcid-numbers              = {{Bursi, Oreste/0000-0003-3072-7414 Bonelli, Alessio/0000-0003-2398-9432}},
  publisher                  = {{ASCE-AMER SOC CIVIL ENGINEERS}},
