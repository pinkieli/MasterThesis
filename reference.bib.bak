}

@Article{Zhang2011,
  author    = {Zhang, Guangbin and Ge, Yunjian},
  title     = {A novel method for gearbox fault detection based on biorthogonal B-spline wavelet},
  journal   = {Sensors and Transducers},
  year      = {2011},
  volume    = {133},
  number    = {10},
  pages     = {83 - 94},
  issn      = {17265479},
  note      = {B-spline wavelet;Biorthogonal;Coefficients line;Compact support;De-noising;Fault characteristics;Fault feature;Fault patterns;Gear fault diagnosis;Gearbox;Hilbert transform;Integrated approach;Localized defects;Periodic impulse;Power spectrum density;Simulation studies;Singularity signals;System Dynamics;Time-frequency distributions;Vanishing moment;Vibration signal;},
  language  = {English},
  abstract  = {Localized defects of gearbox tend to result in periodic impulses in the vibration signal, which contain important information for system dynamics analysis. So parameter identification of impulse provides an effective approach for gearbox fault diagnosis. Biorthogonal B-spline wavelet has the properties of compact support, high vanishing moment and symmetry, which are suitable to signal de-noising, fast calculation, and reconstruction. Thus, a novel time frequency distribution method is present for gear fault diagnosis by biorthogonal B-spline wavelet. Simulation study concerning singularity signal shows that this wavelet is effective in identifying the fault feature with coefficients map and coefficients line. Furthermore, an integrated approach consisting of wavelet decomposition, Hilbert transform and power spectrum density is used in applications. The results indicate that this method can extract the gearbox fault characteristics and diagnose the fault patterns effectively. &copy; 2011 IFSA.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Fault detection},
  keywords  = {Failure analysis;Interpolation;Signal processing;Splines;Vibration analysis;Wavelet decomposition;},
}

@InProceedings{Wang2011,
  author    = {Wang, Zhan and Yu, Jun and Su, Bida and Xie, Xiaodan},
  title     = {An improved algorithm for surface fitting based on B spline function},
  year      = {2011},
  pages     = {80 - 82},
  address   = {Phuket Island, Thailand},
  note      = {B spline functions;B splines;Basic functions;C-B spline function;Computing time;Explicit form;Improved algorithm;Least Square;Numerical example;Recurrence formulas;Spline fitting;Surface fitting;},
