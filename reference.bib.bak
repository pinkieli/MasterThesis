@Article{Argueelles1999,
  author    = {Argüelles, María Eladia and Alonso, Marta and García Suárez, María del Mar and Barneo, Luis and Sampedro, Andrés and Toyos, Juan R. de los},
  title     = {Performance of thiophilic adsorption chromatography in the purification of rat IgG2b monoclonal antibodies from serum- and protein-free culture supernatants},
  journal   = {Biomedical Chromatography},
