@InProceedings{Selinger2011,
  author    = {Selinger, Jevgenija and Linsen, Lars},
  title     = {Efficient curvature-optimized G2-continuous path generation with guaranteed error bound for 3-axis machining},
  year      = {2011},
  pages     = {519 - 527},
  address   = {London, United kingdom},
  note      = {approximation;B-spline curve;Error bound;High quality;Integral part;Local curvature;Machining Process;Off-line algorithm;Path generation;Piecewise linear;Post processing;Pre-processing step;Processing steps;Processing Time;Real-world problem;Simple groups;Small groups;tool path optimization;},
  abstract  = {Path generations are a necessary integral part of any automated machining approach using 3-axis robots. Given an input path in form of a piecewise linear curve, we automatically generate an optimized path that lies within a given error bound or tolerance band of the input path. The optimization is targeted at minimizing the processing time of the machining process. As sharp turns require the robot to slow down, we want to minimize the local curvature at each point of the curve. Our approach is an efficient offline algorithm that consists of several processing steps. Ina preprocessing step, we analyze the input path and split it into small groups. The groups are categorized and can be handled independently and locally. We apply a local sleeve concept for complicated groups and a local Bezierapproximation for simple groups. In a post processing step the groups are combined to form a G<sup>2</sup>-continuous path. Our approach achieves high-quality results that are comparable to the sleeves approach while being significantly more efficient (speed-up of one order of magnitude) when applied to real-world problems. &copy; 2011 IEEE.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  issn      = {10939547},
  journal   = {Proceedings of the International Conference on Information Visualisation},
  key       = {Curve fitting},
  keywords  = {Optimization;Piecewise linear techniques;Splines;Visualization;},
  language  = {English},
  url       = {http://dx.doi.org/10.1109/IV.2011.31},
}

@Article{Ha2011,
  author    = {Ha, Youn Doh and Kim, Min-Geun and Kim, Hyun-Seok and Cho, Seonho},
  title     = {Shape design optimization of SPH fluid-structure interactions considering geometrically exact interfaces},
  journal   = {Structural and Multidisciplinary Optimization},
  year      = {2011},
  volume    = {44},
  number    = {3},
  pages     = {319 - 336},
  issn      = {1615147X},
  note      = {B-spline;B-spline basis function;Compressible Navier-Stokes equations;Control point;Equilibrium equation;Explicit time integration;Fluid-structure interaction problem;Geometric information;Geometrically exact;Geometrically exact interface;Geometrically nonlinear structures;Gradient based;High order;Response analysis;Shape design optimization;Shape designs;Shape optimization problem;Single-step;Smoothed particle hydrodynamics;Smoothed particle hydrodynamics methods;Updated Lagrangian formulations;},
  language  = {English},
  abstract  = {Fluid-structure interaction problems are solved by applying a smoothed particle hydrodynamics method to a weakly compressible Navier-Stokes equation as well as an equilibrium equation for geometrically nonlinear structures in updated Lagrangian formulation. The geometrically exact interface, consisting of B-spline basis functions and the corresponding control points, includes the high order geometric information such as tangent, normal, and curvature. The exactness of interface is kept by updating the control points according to the kinematics obtained from response analysis. Under the scheme of explicit time integration and updated Lagrangian formulation, the required shape design velocity should be updated at every single step. The update scheme of design velocity is developed using the sensitivity of physical velocity. The developed sensitivity analysis method is further utilized in gradient-based shape optimization problems and turns out to be very efficient since the interaction pairs of particles determined in the response analysis can be directly utilized. &copy; 2011 Springer-Verlag.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Shape optimization},
  keywords  = {Design;Fluid structure interaction;Fluids;Hydrodynamics;Lagrange multipliers;Navier Stokes equations;Nonlinear equations;Sensitivity analysis;Splines;Velocity;},
  url       = {http://dx.doi.org/10.1007/s00158-011-0645-8},
}

@InProceedings{Jia2012,
