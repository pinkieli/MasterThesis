  groups     = {B-spline and time integration},
  key        = {Structural dynamics},
  keywords   = {Algorithms;Calculations;Structural analysis;Structural design;, read},
}

@InProceedings{Biagiotti2015,
  author    = {Biagiotti, Luigi and Moriello, Lorenzo and Melchiorri, Claudio},
  title     = {A repetitive control scheme for industrial robots based on b-spline trajectories},
  year      = {2015},
  volume    = {2015-December},
  pages     = {5417 - 5422},
  address   = {Messeplatz 1, Hamburg, Germany},
  note      = {Automatic machines;B-spline trajectory;Interpolation error;Reference trajectories;Repetitive control;Splines (mathematics);Trajectory generation;Unmodelled dynamics;},
  abstract  = {In this paper, a novel repetitive control scheme is presented and discussed. The general framework is the control of repetitive tasks of robotic systems or, more in general, of automatic machines. The key idea of the proposed scheme consists in modifying the reference trajectory provided to the plant in order to compensate for external loads or unmodelled dynamics that cyclically affect it. By exploiting the dynamic filters for the B-spline trajectory planning, it has been possible to integrate the trajectory generation within a repetitive control scheme able to modify in real-time the reference signal with the aims of nullify interpolation errors. Experimental results obtained controlling two joints of a standard industrial manipulator are reported, showing the effectiveness of the proposed method. &copy; 2015 IEEE.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  issn      = {21530858},
  journal   = {IEEE International Conference on Intelligent Robots and Systems},
  key       = {Interpolation},
  keywords  = {FIR filters;Frequency response;Gas generators;Impulse response;Industrial manipulators;Industrial robots;Intelligent robots;Robotics;Robots;Splines;Standards;Trajectories;},
  language  = {English},
  url       = {http://dx.doi.org/10.1109/IROS.2015.7354143},
}

@Article{Pan2009,
  author    = {Pan, Rijing and Yao, Zhiqiang},
  title     = {Biorthogonal nonuniform B-spline wavelets based on a discrete norm},
  journal   = {Computer Aided Geometric Design},
  year      = {2009},
  volume    = {26},
  number    = {4},
  pages     = {480 - 492},
  issn      = {01678396},
  note      = {B-spline wavelets;Biorthogonal;Discrete norm;Multiresolution;Nonuniform B-spline;},
  language  = {English},
  abstract  = {This paper utilizes a recursive formula for transformation matrices of B-spline bases to construct biorthogonal nonuniform B-spline wavelets based on discrete norm l<inf>2</inf> and proposes the reconstruction and decomposition algorithms for the MRA of the wavelets. The computation of reconstruction matrices of the proposed wavelets is based on simple recursion and integral operation is avoided. The reconstruction and decomposition algorithms are both simple, efficient and with linear time complexity. Moreover, the analysis for the performances of the proposed wavelets reveals that the wavelets possess smaller supports in general cases compared with that of semiorthogonal nonuniform B-spline wavelets, with good approximation property and suitable for any uniform and nonuniform nested sequences of knot vectors. Several examples are given for the applications of the proposed wavelets in the MRA of nonuniform B-spline curves. The experimental results show that as any other nonuniform B-spline wavelets, the proposed wavelets provide greater flexibility than uniform B-spline wavelets do for the applications of the MRA of B-spline curves and surfaces. With the proposed wavelets we can easily construct rich multiresolution levels and maintain the specified characteristic points as well as local details of the decomposed curves to satisfy various requirements in the applications. &copy; 2008 Elsevier B.V. All rights reserved.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Splines},
  keywords  = {Applications;Curve fitting;Graph theory;Multiresolution analysis;Restoration;},
  url       = {http://dx.doi.org/10.1016/j.cagd.2008.11.001},
}

@Article{Feng2016,
  author    = {Feng, De-Shan and Wang, Xun},
  title     = {The GPR simulation of bi-phase random concrete medium using finite element of B-spline wavelet on the interval},
  journal   = {Chinese Journal of Geophysics (Acta Geophysica Sinica)},
  year      = {2016},
  volume    = {59},
  number    = {8},
  pages     = {3098 - 3109},
  issn      = {00015733},
  note      = {B-spline wavelet on the intervals;Forward modeling;Ground Penetrating Radar;Random medium;Wavelet finite element method;},
  language  = {Chinese},
  abstract  = {Based on the separable wavelet theory, the scaling functions of one-dimensional B-spline wavelet on the interval (BSWI) is employed to construct the two-dimensional B-spline wavelet bases on the interval. In order to solve the GPR wave equation, these constructed wavelet bases are used as the interpolation functions, and a transformation matrix is introduced to convert between the wavelet coefficient space and the physical space (radar electromagnetic field). In this study, the discrete format of GPR wave equation for two-dimensional interval B-spline wavelet finite element is derived using Galerkin algorithm. And the integral values and the connection coefficients of the second order one-scale and second order two-scale BSWI functions are calculated. The detailed process of the algorithm is given. Then two typical examples are forward modeled using the BSWI method by Matlab program. The result shows that the BSWI with fewer units has the same precision as FEM. Increasing the scale of BSWI algorithm, the precision of the results is also improved, but it is time-consuming. Finally, the BSWI algorithm is applied to model bi-phase random concrete medium, and it proves that the random medium model theory can describe the practical distribution of concrete medium flexibly and effectively. Furthermore, this study found that the forward modeling profile is more accordant with practical profile, which demonstrates that medium model theory can simulate the transmission process of the radar wave more accurately. It also provides a theoretical basis for improving the detection results and interpretation accuracy of GPR. &copy; 2016, Science Press. All right reserved.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Finite element method},
  keywords  = {Algorithms;Concretes;Electromagnetic fields;Geological surveys;Ground penetrating radar systems;Interpolation;Linear transformations;MATLAB;Radar;Space-based radar;Splines;Wave equations;Wavelet analysis;},
  url       = {http://dx.doi.org/10.6038/cjg20160832},
}

