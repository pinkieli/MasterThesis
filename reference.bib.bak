@Article{Jackiewicz1995,
  author   = {Z. Jackiewicz and R. Vermiglio and M. Zennaro},
  title    = {Variable stepsize diagonally implicit multistage integration methods for ordinary differential equations},
  journal  = {Applied Numerical Mathematics},
  year     = {1995},
