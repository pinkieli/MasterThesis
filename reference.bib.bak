@Article{Yi2009a,
  author    = {Yi, Yang and Shen, Hong and Guo, Lei},
  title     = {Statistic PID tracking control for non-Gaussian stochastic systems based on T-S fuzzy model},
  journal   = {International Journal of Automation and Computing},
  year      = {2009},
  volume    = {6},
  number    = {1},
  pages     = {81 - 87},
  issn      = {14768186},
  note      = {B-spline approximation;B-spline neural networks;Control framework;Gaussian;Matrix inequalities;Non-Gaussian systems;Performance functions;PID controllers;Proportional-integral-derivative control;Statistic tracking control;T-S fuzzy model;},
  language  = {English},
  abstract  = {A new robust proportional-integral-derivative (PID) tracking control framework is considered for stochastic systems with non-Gaussian variable based on B-spline neural network approximation and T-S fuzzy model identification. The tracked object is the statistical information of a given target probability density function (PDF), rather than a deterministic signal. Following B-spline approximation to the integrated performance function, the concerned problem is transferred into the tracking of given weights. Different from the previous related works, the time delay T-S fuzzy models with the exogenous disturbances are applied to identify the nonlinear weighting dynamics. Meanwhile, the generalized PID controller structure and the improved convex linear matrix inequalities (LMI) algorithms are proposed to fulfil the tracking problem. Furthermore, in order to enhance the robust performance, the peak-to-peak measure index is applied to optimize the tracking performance. Simulations are given to demonstrate the efficiency of the proposed approach. &copy; 2009 Institute of Automation, Chinese Academy of Sciences and Springer-Verlag GmbH.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Probability density function},
  keywords  = {Control theory;Delay control systems;Functions;Fuzzy control;Gaussian noise (electronic);Identification (control systems);Linear control systems;Linear matrix inequalities;Navigation;Neural networks;Proportional control systems;Random processes;Splines;Stochastic control systems;Stochastic models;Stochastic systems;Trellis codes;Two term control systems;},
  url       = {http://dx.doi.org/10.1007/s11633-009-0081-z},
}

@Article{Rizos2006,
  author    = {Rizos, D.C. and Zhou, S.},
