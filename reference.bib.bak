@Article{Mancuso,
  author    = {Mancuso, Massimo and Ubertini, Francesco},
  title     = {Effective implementation of a time discontinuous Galerkin method},
  abstract  = {Abstract This work deals with time integration methods for linear dynamics. The semidiscrete equations are generally derived from standard finite element procedures and only a few low-frequency modes are accurate approximations of the underlying continuous problem. Thus, the quality of simulations depends crucially on the accuracy properties of the time integration method in the low-frequency modes, as well as on the dissipative properties in the high-frequency ones. The Time Discontinuous Galerkin (TDG) method possesses these desirable properties and has been successfully applied to linear dynamics (see e.g. (1)). Despite its good properties, it typically leads to systems of coupled equations which are larger than the original one, so that standard implementations are very demanding for both storage and computational work. Thus, developing effective implementations plays a key role in the practical application of the method. The purpose of this work is to establish an efficient iterative procedure to implement the TDG method. Differently from previous implementations (see e.g. (2) and (3)), the present procedure is designed to generate a sequence of updated values for displacements and velocities, by searching for the TDG solution while preserving the TDG stability and dissipative properties at each iteration. Improving only accuracy is demanded to iterative corrections. The idea for the iterative scheme stems from the implementation of the N酶rsett methods (4). The scheme is established so to obtain at the first iteration a N酶rsett approximant with the target stability and dissipative properties. The resultant procedure is simple and offers several advantages. Remarkable computational savings are achieved since the iterative corrections are performed using the same iteration matrix, which retains the structure of the original system and can be factorized only once for each fixed time step size. No more than two iterations are needed to obtain the same overall accuracy of the TDG method. Second order accuracy is achieved at the first iteration and third order accuracy at the second one. References},
  groups    = {Dynamic equation numerical methods},
  owner     = {Pinkie.li},
  timestamp = {2017-11-14},
}

@Article{Mancusoa,
  author    = {Mancuso, M. and Ubertini, F.},
  title     = {Effective higher-order methods for linear dynamics},
  abstract  = {ABSTRACT  This work presents a simple and efficient iterative procedure aimed at improving the properties of N酶rsett methods for linear dynamics towards those of Pad茅 methods. The procedure is used to implement a set of higher-order methods with controllable dissipation, recently presented by the authors. Some numerical tests demonstrate the effectiveness of the proposed implementation. Competitive time stepping methods for finite element analysis in linear dynamics should possess certain peculiar attributes. In addition to unconditional stability, the salient properties are higher-order accuracy and controllable dissipation in the spurious high modes. Higher-order accuracy is crucial for long-term simulations in order to avoid an excessive accumulation of error. Moreover, some amount of dissipation in the higher modes is desirable in order to damp out their response, as it is well known that only a few low-frequency modes of the semidiscrete problem are accurate approximations to the modes of the original continuous problem. Indeed, the dissipative properties of most of the established A-and L-stable methods can be unsatisfactory. A-stable methods can damp out the high-frequency modes too slowly and spurious effects can show up. On the other hand, L-stable methods can introduce excessive dissipation in the low-mode response and this can affect the accuracy especially in long-term simulations. Therefore, a specific algorithmic parame-ter should be available to control the amount of numerical dissipation according to the importance of the spurious modal components.},
  groups    = {Dynamic equation numerical methods},
  owner     = {Pinkie.li},
  timestamp = {2017-11-14},
}

