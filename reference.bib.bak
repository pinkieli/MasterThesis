  groups    = {J.C. Butcher, General linear method},
  key       = {Nonlinear equations},
  keywords  = {Boundary conditions;Iterative methods;Mathematical techniques;Newton-Raphson method;Numerical methods;Ordinary differential equations;Partial differential equations;},
  owner     = {Pinkie.li},
  timestamp = {2017-09-14},
}

@Article{Datta2004,
  author    = {Datta, Dhurjati Prasad and Bose, Manoj Kumar},
