  key       = {Integration},
  keywords  = {Computational efficiency;Jacobian matrices;Runge Kutta methods;Structural dynamics;},
  language  = {English},
  url       = {http://dx.doi.org/10.1007/978-3-319-04522-1_25},
}

@InProceedings{Rahrovani2016,
