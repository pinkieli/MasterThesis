@Article{Butcher1992,
  author   = {J.C. Butcher},
  title    = {The adaptation of \{STRIDE\} to delay differential equations},
  journal  = {Applied Numerical Mathematics},
  year     = {1992},
  volume   = {9},
  number   = {3–5},
  pages    = {415 - 425},
  issn     = {0168-9274},
  abstract = {The differential equation solver \{STRIDE\} is at present being modified to make it more convenient to use, especially for difficult and nonstandard problems. In particular, its possible use for delay differential equations is being taken into account in the design. What seems to be needed is a means of calculating approximate solution values outside a current step by interpolation from information generated when previous steps were being worked through. In a solver of conventional design, there is also needed a means of making this data from previous steps available to the subroutine which computes the right-hand side of the delay differential equation. In STRIDE, interpolation within a current step is carried out using an expansion in a modified form of Taylor series, in which Laguerre polynomials take the place of the usual powers of the offset from the beginning of the current step. This is inappropriate for later use in computing delays and part of the content of this paper deals with a transformation of the data to a more convenient form. The other main topic discussed will be the use of reverse communication to allow flexibility of usage and to allow the extension to delay problems particularly straightforward. },
  doi      = {http://dx.doi.org/10.1016/0168-9274(92)90031-8},
  groups   = { General linear method},
  url      = {http://www.sciencedirect.com/science/article/pii/0168927492900318},
}

