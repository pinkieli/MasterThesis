}

@Article{Weiner2008,
  author   = {Rüdiger Weiner and Katja Biermann and Bernhard A. Schmitt and Helmut Podhaisky},
  title    = {Explicit two-step peer methods},
  journal  = {Computers \& Mathematics with Applications},
  year     = {2008},
  volume   = {55},
  number   = {4},
  pages    = {609 - 619},
  issn     = {0898-1221},
  abstract = {We present a new class of explicit two-step peer methods for the solution of nonstiff differential systems. A construction principle for methods of order p = s , s the number of stages, with optimal zero-stability is given. Two methods of order p = 6 , found by numerical search, are tested in Matlab on several representative nonstiff problems. The comparison with \{ODE45\} confirms the high potential of the new class of methods.},
