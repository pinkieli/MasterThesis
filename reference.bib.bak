@Article{Hughes1979159,
  author   = {Thomas J.R. Hughes and Karl S. Pister and Robert L. Taylor},
  title    = {Implicit-explicit finite elements in nonlinear transient analysis},
  journal  = {Computer Methods in Applied Mechanics and Engineering},
