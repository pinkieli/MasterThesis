  file      = {:Grolleau2004.pdf:PDF},
  groups    = {Dynamic equation numerical methods},
  keywords  = {Mécanique des solides numérique, Explicite, Eléments finis, Bruit numérique, Amortissement, Dynamique, Computational solid mechanics, Explicite, Finite element, Numerical noise, Dissipation, Dynamics},
  owner     = {Pinkie.li},
