@InProceedings{Cottrell2006a,
  author               = {Cottrell, J. A. and Reali, A. and Bazilevs, Y. and Hughes, T. J. R.},
  title                = {Computational geometry and the analysis of solids and structures},
  booktitle            = {Computational Mechanics: Solids, Structures and Coupled Problems},
  year                 = {2006},
  editor               = {Soares, CAM and Marting, JAC and Rodrigues, HC},
  volume               = {6},
  series               = {Computational Methods in Applied Sciences},
  pages                = {21+},
  organization         = {IACM; ECCOMAS; ECCSM; APMTAC; Fdn Sci \& Technol; Calouste Gulbenkian Fdn; Tech Univ Lisbon; Inst Super Tecnico},
  note                 = {3rd European Conference on Computational Mechanics, Natl Civil Engn Lab, Lisbon, PORTUGAL, JUN 04-09, 2006},
  abstract             = {After reviewing some fundamentals of Isogeometric Analysis, application
   is made to structural vibration models, namely rods and thin beams. The
   concept of k-refinement is explored and shown to produce more accurate
   and robust results than corresponding finite elements. Through the use
   of nonlinear parameterization, ``optical{''} branches of frequency
   spectra are eliminated for k-refined meshes. Optical branches have been
   identified as contributors to Gibbs phenomena in wave propagation
   problems and the cause of rapid degradation of higher modes in p-method
   finite elements. A geometrically exact model of the NASA Aluminum
   Testbed Cylinder is constructed and frequencies and mode shapes are
   computed and shown to compare favorably with experimental results.},
  doi                  = {10.1007/978-1-4020-4979-8\_2},
