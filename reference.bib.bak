}

@Article{Rouzaud2017,
  author    = {Rouzaud, C. and Gatuingt, F. and Herve, G. and Dorival, O.},
  title     = {A new multi-frequency approach based on Pade approximants for the treatment of transient dynamics problems with the variational theory of complex rays},
  journal   = {Journal of Sound and Vibration},
  year      = {2017},
  volume    = {392},
  pages     = {170 - 186},
  issn      = {0022460X},
  note      = {Approximants;Civil engineering structures;Frequency resolutions;Frequency response functions;Model reduction;Transient dynamic problems;Variational theory of complex rays;VTCR;},
  language  = {English},
  abstract  = {Frequency-based methods were set up in order to circumvent the limits of classical finite element methods in fast dynamic simulations due to discretizations. In this approach the dynamic loading was shifted in the frequency domain by FFT, then treated by the Variational Theory of Complex Rays, and then the time response was reconstructed through an IFFT. This strategy proved to be very efficient due to the CPU VTCR very low cost. However in the case of a large loading spectrum this frequency-by-frequency approach could seriously degrade the computational performances of the strategy. This paper addresses this point by proposing the use of Pad&eacute; approximants in order to limit the number of frequencies at which the response should be calculated. Pad&eacute; approximation is applied to the overall VTCR system based on its frequency dependency. Finally, as simulations on a simple academic case and on a civil engineering structure show, this method is found to be very efficient for interpolating the frequency response functions of a complex structure. This is a key point to preserve the efficiency of the complete VTCR strategy for transient dynamic problems. &copy; 2016 Elsevier Ltd},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {Dynamic equation numerical methods},
  key       = {Computation theory},
  keywords  = {Dynamic loads;Finite element method;Frequency domain analysis;Frequency response;Structural analysis;Transient analysis;},
  owner     = {Pinkie.li},
  timestamp = {2017-09-15},
  url       = {http://dx.doi.org/10.1016/j.jsv.2016.12.026},
}

@Article{Ruge2001,
  author    = {Ruge, P.},
  title     = {Restricted Pade scheme in computational structural dynamics},
  journal   = {Computers and Structures},
  year      = {2001},
  volume    = {79},
  number    = {20-21},
  pages     = {1913 - 1921},
  issn      = {00457949},
  note      = {Structural dynamics;},
  language  = {English},
  abstract  = {Restricted Pade&acute; schemes R<inf>mp</inf> where the denominator is a complete power p of a linear function have been examined analytically by No&die;rsett. Here this method is evaluated with paying special attention to numerical aspects when solving typical finite-element systems in structural dynamics with n degrees of freedom and with essential properties like sparsety and symmetry. This paper presents an efficient implicit restricted time-stepping scheme R<inf>22</inf> with a final coefficient matrix of order n and a leading error term proportional to h<sup>4</sup>. This is a significant improvement compared with the most popular Pade&acute;-P<inf>11</inf>-Newmark scheme with matrix order n, too, but error term h<sup>3</sup> on the one hand and the well established Pade&acute;-P<inf>12</inf>-discontinuous Hamiltonian approach with error term h<sup>4</sup> but matrix order 2 &times; n on the other hand. The classical P<inf>11</inf>-scheme and the new restricted R<inf>22</inf>-scheme are compared in a modal manner and by assessing the correlation between local error estimator and global error. &copy; 2001 Published by Elsevier Science Ltd.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {Dynamic equation numerical methods},
  key       = {Structural analysis},
  keywords  = {Algorithms;Computational methods;Computer simulation;Correlation methods;Damping;Degrees of freedom (mechanics);Error analysis;Finite element method;Hamiltonians;Mathematical models;Matrix algebra;Subroutines;Vibrations (mechanical);},
  owner     = {Pinkie.li},
