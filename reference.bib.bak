@Article{Chang2017b,
  author                     = {Chang, Shuenn-Yih},
  title                      = {A Loading Correction Scheme for a Structure-Dependent Integration Method},
  journal                    = {{JOURNAL OF COMPUTATIONAL AND NONLINEAR DYNAMICS}},
  year                       = {{2017}},
  volume                     = {{12}},
  number                     = {{1}},
  month                      = {{JAN}},
  issn                       = {{1555-1423}},
  language                   = {{English}},
  abstract                   = {{A structure-dependent integration method may experience an unusual
   overshooting behavior in the steady-state response of a high frequency
   mode. In order to explore this unusual overshooting behavior, a local
   truncation error is established from a forced vibration response rather
   than a free vibration response. As a result, this local truncation error
   can reveal the root cause of the inaccurate integration of the
   steady-state response of a high frequency mode. In addition, it
   generates a loading correction scheme to overcome this unusual
   overshooting behavior by means of the adjustment the difference equation
   for displacement. Apparently, these analytical results are applicable to
   a general structure-dependent integration method.}},
