@Article{Zheng1997,
  author                     = {Zheng, ZC and Ren, GX and Wang, WJ},
  title                      = {{A reduction method for large scale unsymmetric eigenvalue problems in structural dynamics}},
  journal                    = {{JOURNAL OF SOUND AND VIBRATION}},
  year                       = {{1997}},
  volume                     = {{199}},
  number                     = {{2}},
  pages                      = {{253-268}},
  month                      = {{JAN 16}},
  issn                       = {{0022-460X}},
  abstract                   = {{The discussion begins with the classification of eigenvalue problems
   arising from conservative and non-conservative structural systems. The
   conservative type includes undamped structural eigenvalue problems and
   undamped gyroscopic eigenvalue problems. The non-conservative type
   includes damped structural eigenvalue problems, damped gyroscopic
   eigenvalue problems and constrainedly damped eigenvalue problems. The
   methods for solving large scale unsymmetric eigenvalue problems are
   briefly reviewed. The advantages and properties of Arnoldi's method have
   also been discussed. Arnoldi's reduction method has been generalized and
   the partial solution of large scale unsymmetric-definite eigenvalue
   problems in structural dynamics is presented in detail. A very simple
   reduction algorithm is obtained by simplifying the proposed method for
   undamped gyroscopic eigenvalue problems. To make the proposed reduction
   method feasible for engineering problems, a restart technique is
   introduced to work with Arnoldi's reduction method for checking and
   computing missing eigenvalues. Numerical examples are also presented to
   demonstrate the effectiveness of the proposed reduction method. (C) 1997
   Academic Press Limited.}},
  affiliation                = {{Zheng, ZC (Reprint Author), TSING HUA UNIV, DEPT ENGN MECH, BEIJING 100084, PEOPLES R CHINA. DE MONTFORT UNIV, DEPT MECH \& MFG ENGN, LEICESTER LE1 9BH, LEICS, ENGLAND.}},
  cited-references           = {{ARNOLDI WE, 1951, Q APPL MATH, V9, P17. Bathe K.-J., 1973, International Journal for Numerical Methods in Engineering, V6, DOI 10.1002/nme.1620060207. BAUCHAU OA, 1986, INT J NUM MET ENG, V25, P1705. Golub G H, 1989, MATRIX COMPUTATIONS. GRIMES RG, 1986, EIGENVALUE PROBLEMS, P91. GUPTA KK, 1988, INT J NUMER METH ENG, V26, P1029, DOI 10.1002/nme.1620260503. Huseyin K., 1978, VIBRATIONS STABILITY. HYOUNG MK, 1988, INT J NUMER METH ENG, V26, P2305. Jennings A., 1992, MATRIX COMPUTATIONS. KLIEM W, 1986, Q J APPL MATH, V43, P456. LANCZOS C, 1950, J RES NAT BUR STAND, V45, P255, DOI 10.6028/jres.045.026. MEIROVIT.L, 1974, AIAA J, V12, P1337, DOI 10.2514/3.49486. Meirovitch L., 1980, COMPUTATIONAL METHOD. MINGORI DL, 1970, J APPL MECH, V37, P253. Paige C. C., 1970, BIT (Nordisk Tidskrift for Informationsbehandling), V10, P183, DOI 10.1007/BF01936866. Parlett B. N., 1979, Mathematics of Computation, V33, P217, DOI 10.2307/2006037. RAJAKUMAZ C, 1993, COMPUTER METHODS APP, V105. REN GX, 1995, POSTDOCTORAL RES REP. REN GX, 1993, THESIS TSINGHUA U. Saad Y., 1992, NUMERICAL METHODS LA. SAAD Y, 1982, SIAM J NUMER ANAL, V19, P485, DOI 10.1137/0719031. SAAD Y, 1980, LINEAR ALGEBRA APPL, V34, P269, DOI 10.1016/0024-3795(80)90169-X. STEWART GW, 1976, NUMER MATH, V25, P123, DOI 10.1007/BF01462265. TASSO H, 1980, Z ANGEW MATH PHYS, V31, P536, DOI 10.1007/BF01590865. TEICHMAN J, 1979, Z ANGEW MATH PHYS, V30, P1027, DOI 10.1007/BF01590501. Wilkinson J. H., 1965, ALGEBRAIC EIGENVALUE. ZHENG ZC, 1995, IN PRESS INT J MECH. ZHENG ZC, 1995, P INT C STRUCT DYN V, P125.}},
