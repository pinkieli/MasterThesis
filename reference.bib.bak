  keywords  = {Dynamical systems;Estimation;Least squares approximations;Maximum likelihood estimation;Newton-Raphson method;Nonlinear dynamical systems;Numerical methods;Optimization;Ordinary differential equations;},
  owner     = {Pinkie.li},
