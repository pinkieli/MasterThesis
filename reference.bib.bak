@InProceedings{Arriagada2008,
  author    = {Arriagada, Ignacio A. and Yuz, Juan I.},
  title     = {On the relationship between splines, sampling zeros and numerical integration in sampled-data models for linear systems},
  year      = {2008},
  pages     = {3665 - 3670},
  address   = {Seattle, WA, United states},
  note      = {B splines;Continuous time modeling;Continuous-time;Integration strategies;Numerical integrations;Real systems;Sampled-data;Sampling zeros;},
  abstract  = {Real systems are usually modelled in continuous-time by differential equations but, in practice, we have to deal with them through digital devices using sampled data. For linear systems, exact sampled-data models can be obtained. These models, however, will generally have more zeros than the continuous-time model. In this paper we show that there is a specific relation between the characterization of these sampling zeros and the order of the B-splines used to generate the continuous-time input to the system. Moreover, we show that this connection can also be established if the smoothness of the B-spline is taken into account in the integration strategy used to discretize the continuous-time model. &copy;2008 AACC.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  issn      = {07431619},
  journal   = {Proceedings of the American Control Conference},
  key       = {Continuous time systems},
  keywords  = {Difference equations;Differential equations;Differentiation (calculus);Digital devices;Integration;Learning systems;Linear systems;Numerical analysis;Real time systems;Ship propellers;Splines;},
  language  = {English},
  url       = {http://dx.doi.org/10.1109/ACC.2008.4587063},
}

@Article{Buchmann2000,
  author    = {Buchmann, Bjarne},
  title     = {Accuracy and stability of a set of free-surface time-domain boundary element models based on B-splines},
  journal   = {International Journal for Numerical Methods in Fluids},
  year      = {2000},
  volume    = {33},
  number    = {1},
  pages     = {125 - 155},
