  file      = {:the general linear methods\\Pessanha2013.pdf:PDF},
  groups    = {BDF},
  key       = {Iterative methods},
  keywords  = {Differential equations;Jacobian matrices;},
  owner     = {Pinkie.li},
  timestamp = {2017-11-15},
  url       = {http://dx.doi.org/10.1016/j.ijepes.2012.08.065},
}

@InProceedings{Peinado2010,
  author    = {Peinado, Jesus and Ibanez, Javier and Hernandez, Vicente and Arias, Enrique},
