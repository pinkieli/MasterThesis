@Article{Muraleetharan1994,
  author    = {Muraleetharan, K. K. and Mish, K. D. and Arulanandan, K.},
  title     = {A fully coupled non-linear dynamic analysis procedure and its verification using centrifuge test results},
  journal   = {International Journal for Numerical and Analytical Methods in Geomechanics},
  year      = {1994},
  volume    = {18},
  number    = {5},
  pages     = {305--325},
  issn      = {1096-9853},
  abstract  = {The equations governing the dynamic behavior of saturated porous media as well as a finite element spatial discretization of these equations are summarized. A three-parameter time integration scheme called the Hilber–Hughes–Taylor α-method is used together with a predictor/multi-corrector algorithm, instead of the widely used Newmark's method, to integrate the spatially discrete finite element equations. The new time integration scheme possess quadratic accuracy and desirable numerical damping characteristics. The proposed numerical solution and bounding surface plasticity theory to describe the constitutive behaviour of soil have been implemented as the computer code DYSAC2. Predictions made by DYSAC2 code are verified using dynamic centrifuge test results for a clay embankment. Importance of initial state of a soil on its dynamic behaviour is demonstrated.},
