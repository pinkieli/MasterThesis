  unique-id            = {{ISI:000274827400007}},
}

@Article{Auricchio2010,
  author               = {Auricchio, F. and Da Veiga, L. Beirao and Hughes, T. J. R. and Reali, A. and Sangalli, G.},
  title                = {{ISOGEOMETRIC COLLOCATION METHODS}},
  journal              = {{MATHEMATICAL MODELS \& METHODS IN APPLIED SCIENCES}},
