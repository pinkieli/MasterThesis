}

@Article{Tooth1993,
  author    = {Tooth, Leigh and McKenna, Kryss and Colquhoun, David},
  title     = {Prediction of Compliance with a Post-Myocardial Infarction Home-Based Walking Programme},
  journal   = {Australian Occupational Therapy Journal},
  year      = {1993},
  volume    = {40},
  number    = {1},
  pages     = {17--22},
  issn      = {1440-1630},
  abstract  = {This study was undertaken to determine the personal, medical and psychological factors which may predict non-compliance with a home-based, moderate-intensity walking programme in post-infarction patients. Thirty male patients, aged 32 to 75 years, underwent a six week walking programme. After an introductory interview, the patients completed a self-administered questionnaire and maintained an exercise log book, indicating exercise frequency and duration for a six week period. Compliance was expressed as percentage duration and percentage frequency for the total six week period. The results showed an 86 per cent compliance rate to the exercise programme. It was concluded that such training is a valuable and economical strategy in cardiac rehabilitation programmes.},
  doi       = {10.1111/j.1440-1630.1993.tb01771.x},
  groups    = {W.L.WOOD},
  keywords  = {rehabilitation – cardiac, therapeutic exercise, patient compliance},
  publisher = {Blackwell Publishing Ltd},
  url       = {http://dx.doi.org/10.1111/j.1440-1630.1993.tb01771.x},
}

@Article{Boisse1990,
  author    = {Boisse, Ph. and Bussy, P. and Ladeveze, P.},
  title     = {A new approach in non-linear mechanics: The large time increment method},
  journal   = {International Journal for Numerical Methods in Engineering},
  year      = {1990},
  volume    = {29},
  number    = {3},
  pages     = {647--663},
  issn      = {1097-0207},
  abstract  = {A new algorithm family taking into account the entire loading process in a single large time increment is proposed to compute structures with physical non-linearities and is tested on some examples in elastoplasticity. The method considerably reduces the number of transfers between local and global levels, hence the numerical cost of calculation is also diminished.},
