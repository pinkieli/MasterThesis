@Article{Butcher1996b,
  author   = {J.C. Butcher and G. Wanner},
  title    = {Runge-Kutta methods: some historical notes},
  journal  = {Applied Numerical Mathematics},
  year     = {1996},
  volume   = {22},
  number   = {1–3},
  pages    = {113 - 151},
  issn     = {0168-9274},
  note     = {Special Issue Celebrating the Centenary of Runge-Kutta Methods},
  abstract = {This centenary history of Runge-Kutta methods contains an appreciation of the early work of Runge, Heun, Kutta, and Nyström and a survey of some significant developments of these methods over the last hundred years. In particular, the order conditions, as they are now understood, will be outlined, as will the introduction and practical implementation of implicit Runge-Kutta methods, the use of linear and nonlinear stability analysis in the assessment of Runge-Kutta methods, and the theory and applications of the composition of methods. Of the many further developments that have arisen, most are discussed only briefly; however, the recent interest in Runge-Kutta methods for Hamiltonian systems will be surveyed in more detail. },
