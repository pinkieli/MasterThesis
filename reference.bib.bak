}

@Article{Kim2009b,
  author    = {Kim, Yooil and Kim, Kyoung-Hwan and Kim, Yonghwan},
  title     = {Springing analysis of a seagoing vessel using fully coupled BEM-FEM in the time domain},
  journal   = {Ocean Engineering},
  year      = {2009},
  volume    = {36},
  number    = {11},
  pages     = {785 - 796},
  issn      = {00298018},
  note      = {Accurate prediction;B-spline;BEM-FEM coupling;Convergence speed;Coupling terms;Direct integration;FE equations;Field equation;Finite element equations;Finite element formulations;Fixed-point iterations;Flexible bodies;Fluid motions;Forward speed;Fully-coupled;Higher order;Implicit methods;Modal superposition method;Mode shapes;Newmark methods;Optimum number;Panel methods;Quasi-steady;Rankine;Rankine panel method;Resonant vibrations;Resonant waves;Rigid body;Seagoing vessels;Ship motion;Springing;Stokes theorem;Structural domains;Structural response;Time domain;Warping;},
  language  = {English},
  abstract  = {The quasi-steady resonant vibration of a flexible seagoing vessel under resonant wave excitation force, called springing, is studied in this paper. A higher-order B-spline Rankine panel method is used to represent the effects of the fluid motion surrounding this flexible seagoing vessel, and a finite element formulation based on Vlasov beam is employed for structural response. The boundary integral equation and finite element equation, both for fluid and structural domains, are fully coupled with each other using an iterative implicit method in the time domain. Coupling between the two field equations is achieved by relying on fixed-point iteration with relaxation aided by Aitken's &delta;<sup>2</sup> process to maximize convergence speed. The steady-unsteady coupling term or m-term in the linearized body boundary condition derived by Timman and Newman is taken into account for accurate prediction of flexible body motion when forward speed is present. The 2nd derivative of basis potential in the m-term is obtained by modifying Nakos approach, which was originally developed using the Stokes theorem for rigid body ship motion problem. For the solution of the FE equation, instead of conventionally used modal superposition method, a direct integration scheme based on Newmark method is employed. It is believed that this technique is more attractive in the sense that it allows us free from the selection of optimum number of mode-shapes in the computation. &copy; 2009 Elsevier Ltd. All rights reserved.},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {B-spline and time integration},
  key       = {Boundary element method},
  keywords  = {Boundary integral equations;Coupled circuits;Equations of motion;Finite element method;Fluid structure interaction;Hydrodynamics;Hydroelasticity;Splines;Time domain analysis;Weaving;},
  url       = {http://dx.doi.org/10.1016/j.oceaneng.2009.04.002},
}

@InProceedings{Maleknejad2011,
  author    = {Maleknejad, K. and Nosrati Sahlan, M. and Torabi, P.},
  title     = {Low memory requirement and computational time method for solving a class of integral equations},
  year      = {2011},
  volume    = {3},
  pages     = {104 - 109},
  address   = {Istanbul, Turkey},
  note      = {B-spline wavelet;Compact support;Computational time;Fredholm integral equation;GMRES methods;Low memory;Memory requirement;Operational matrices;Solving sparse systems;Thresholding;Vanishing moment;},
  abstract  = {In this work, we present a new low memory requirement and low computational time method for solving a class of integral equation of the second kind which is based on the use of B-Spline wavelets. Because of vanishing moments and compact support and semiorthogonality properties of these wavelets, operational matrix of the method is very sparse. Also, applying some appropriate thresholding parameter and GMRES method for solving sparse systems, we get a computationally attractive method with low CPU requirement. For showing the accuracy of the method some numerical method are presented. &copy; 2010 Published by Elsevier Ltd.},
