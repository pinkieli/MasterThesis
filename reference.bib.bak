@Article{Tarnow1994,
  author    = {N. Tarnow and J.C. Simo},
  title     = {How to render second order accurate time-stepping algorithms fourth order accurate while retaining the stability and conservation properties},
  journal   = {Computer Methods in Applied Mechanics and Engineering},
  year      = {1994},
  volume    = {115},
  number    = {3},
  pages     = {233 - 252},
  issn      = {0045-7825},
  abstract  = {For a fairly general class of second order accurate algorithms, a sub-stepping procedure is described which achieves fourth order accuracy while retaining the stability, conservation properties and implementation of the underlying second order method. Two features render the proposed approach especially attractive from a computational perspective over conventional fourth order methods. First, the improved (fourth order) accuracy is attained merely by two additional residual evaluations, with no changes in the implementation of the underlying second order method. In particular, no additional storage or extra computations of high order gradients are required. Second, for Hamiltonian systems, the stability properties and possible conservation properties of the underlying second order method, such as energy, momentum and symplectic character, are preserved. The new technique is applied to a number of standard algorithms for elastodynamics, structural and rigid body dynamics. In particular, when applied to the recently proposed (second order accurate) energy-momentum method, this approach results in a fourth order accurate energy, linear and angular momentum conserving algorithm for general Hamiltonian systems.},
  doi       = {10.1016/0045-7825(94)90061-2},
  file      = {:composite method\\Tarnow1994.pdf:PDF},
  groups    = { composite integration method},
  owner     = {Pinkie.li},
  timestamp = {2017-11-18},
  url       = {http://www.sciencedirect.com/science/article/pii/0045782594900612},
}

