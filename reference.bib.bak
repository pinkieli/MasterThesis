@Article{ZIENKIEWICZ1994,
  author    = {ZIENKIEWICZ, OC and WU, J},
  title     = {{AUTOMATIC DIRECTIONAL REFINEMENT IN ADAPTIVE ANALYSIS OF COMPRESSIBLE FLOWS}},
  journal   = {{INTERNATIONAL JOURNAL FOR NUMERICAL METHODS IN ENGINEERING}},
  year      = {{1994}},
  volume    = {{37}},
  number    = {{13}},
  pages     = {{2189-2210}},
  month     = {{JUL 15}},
  issn      = {{0029-5981}},
  abstract  = {{In such local phenomena as shocks and boundary layers, etc, the
   discretization errors are directional and the use of elongated elements
   is obvious if economy is to be achieved. Although facilities for local
   mesh elongation have been incorporated into earlier codes in generating
   unstructured meshes, these do not permit the use of very high stretch
   ratios required for the analysis of such problems as boundary layers.
   Here the use of locally introduced structured meshes has been practised
   involving much manual interaction and loss of error control. In the
   paper we present a procedure which achieves the combination of such
   locally structured meshes with a general unstructured mesh generator in
   an automatic and fully adaptive manner. The procedure is illustrated in
   the context of compressible, viscous and inviscid, flows but has obvious
   applications in other (solid mechanics) problems involving localization.}},
  doi       = {{10.1002/nme.1620371304}},
  groups    = {Zienkiewicz O.C.},
  unique-id = {{ISI:A1994NV78300003}},
}

@Article{Codina2002,
  author               = {Codina, R and Zienkiewicz, OC},
  title                = {{CBS versus GLS stabilization of the incompressible Navier-Stokes equations and the role of the time step as stabilization parameter}},
  journal              = {{COMMUNICATIONS IN NUMERICAL METHODS IN ENGINEERING}},
  year                 = {{2002}},
  volume               = {{18}},
  number               = {{2}},
  pages                = {{99-112}},
  month                = {{FEB}},
  issn                 = {{1069-8299}},
  abstract             = {{In this work we compare two apparently different stabilization
   procedures for the finite element approximation of the incompressible
   Navier-Stokes equations. The first is the characteristic-based split
   (CBS). It combines the characteristic Galerkin method to deal with
   convection dominated flows with a classical splitting technique, which
   in some cases allows us to use equal velocity-pressure interpolations.
   The second approach is the Galerkin-least-squares (GLS) method, in which
   a least-squares form of the element residual is added to the basic
   Galerkin equations. It is shown that both formulations display similar
   stabilization mechanisms, provided the stabilization parameter of the
   GLS method is identified with the time step of the CBS approach. This
   identification can be understood from a formal Fourier analysis of the
   linearized problem. Copyright (C) 2001 John Wiley Sons, Ltd.}},
  doi                  = {{10.1002/cnm.470}},
  groups               = {Zienkiewicz O.C.},
  orcid-numbers        = {{Codina, Ramon/0000-0002-7412-778X}},
  researcherid-numbers = {{Codina, Ramon/I-2311-2014}},
  unique-id            = {{ISI:000173836000003}},
}

@Article{XK1992,
  author    = {LI, XK and ZIENKIEWICZ, OC},
  title     = {{MULTIPHASE FLOW IN DEFORMING POROUS-MEDIA AND FINITE-ELEMENT SOLUTIONS}},
  journal   = {{COMPUTERS \& STRUCTURES}},
  year      = {{1992}},
  volume    = {{45}},
  number    = {{2}},
  pages     = {{211-227}},
  month     = {{OCT 3}},
  issn      = {{0045-7949}},
  abstract  = {{A numerical model to simulate the behaviour of porous media interacting
   with the flow of multiphase immiscible porous fluids is presented. The
   governing equations are derived in the light of the generalized Biot
   theory and a general formulation for effective stress is established.
   Two special cases are discussed in detail. The numerical modelling of
   porous media interacting with two immiscible porous fluids is discussed
   in more detail. The displacement of the solid, the pressure and
   saturation of the wetting fluid are taken as primary unknowns of the
   model. A generalized Galerkin procedure is devised to establish the
   coupled finite element equation set with u-p-S form. Unconditionally
   stable direct and staggered solution procedures are used for the time
   domain numerical solution. Examples are presented to illustrate the
   performance and capability of the approach.}},
  doi       = {{10.1016/0045-7949(92)90405-O}},
  groups    = {Zienkiewicz O.C.},
  unique-id = {{ISI:A1992JV63700001}},
}

@Article{Nithiarasu2000,
  author               = {Nithiarasu, P and Zienkiewicz, OC},
  title                = {{Adaptive mesh generation for fluid mechanics problems}},
  journal              = {{INTERNATIONAL JOURNAL FOR NUMERICAL METHODS IN ENGINEERING}},
  year                 = {{2000}},
  volume               = {{47}},
  number               = {{1-3}},
  pages                = {{629-662}},
  month                = {{JAN 10}},
  issn                 = {{0029-5981}},
  abstract             = {{In this study, we give an in depth investigation of the adaptive
   procedures for compressible and incompressible how problems. Many tests
   have been carried out with different error indicators for different
   problems. These refinement procedures show that the interpolation error
   indicators are adequate to predict an accurate solution. Comparison
   between the gradient and curvature-based error indicators show that the
   former yields better performance in most situations. However, we found
   that the latter indicators predict better motion in the recirculatory
   and unstable regions, than the gradient-based method. Many test cases
   have been presented for both compressible and incompressible flow
   problems. In the later part of the paper, we suggest a simple way of
   combining the curvature/gradient indicators of different key variables
   and present an appropriate problem where it is applicable. A wide
   coverage of relevant literature has also been presented in this paper.
   Copyright (C) 2000 John Wiley \& Sons, Ltd.}},
  doi                  = {{10.1002/(SICI)1097-0207(20000110/30)47:1/3<629::AID-NME786>3.0.CO;2-Y}},
  groups               = {Zienkiewicz O.C.},
  orcid-numbers        = {{Nithiarasu, Perumal/0000-0002-4901-2980}},
  researcherid-numbers = {{Nithiarasu, Perumal/E-9210-2013}},
  unique-id            = {{ISI:000084864500029}},
}

