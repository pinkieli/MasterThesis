}

@Article{Abdi2014,
  author   = {A. Abdi and M. Braś and G. Hojjati},
  title    = {On the construction of second derivative diagonally implicit multistage integration methods for \{ODEs\}},
  journal  = {Applied Numerical Mathematics},
  year     = {2014},
  volume   = {76},
  pages    = {1 - 18},
  issn     = {0168-9274},
  abstract = {Abstract Second derivative diagonally implicit multistage integration methods (SDIMSIMs) as a subclass of second derivative general linear methods (SGLMs) have been divided into four types, depending on the nature of the differential system to be solved and the computer architecture that is used to implement these methods. In this paper, we describe the construction of \{SDIMSIMs\} for all types with Runge–Kutta stability property. Examples of ( p , q , r , s ) \{SDIMSIMs\} are given with p = q = r = s ⩽ 4 , where p is the order, q is the stage order, r is the number of external stages, and s is the number of internal stages of the method. Efficiency of the constructed methods is shown by numerical experiments. },
  doi      = {https://doi.org/10.1016/j.apnum.2013.08.006},
  groups   = {General linear method},
  keywords = {General linear methods, Two-derivative methods, Ordinary differential equation, Order conditions, A- and L-stability, Runge–Kutta stability },
  url      = {http://www.sciencedirect.com/science/article/pii/S0168927413001360},
}

@Article{Schmitt2012,
  author   = {B.A. Schmitt},
  title    = {On algebraic stability of general linear methods and peer methods},
  journal  = {Applied Numerical Mathematics},
  year     = {2012},
  volume   = {62},
  number   = {10},
  pages    = {1544 - 1553},
  issn     = {0168-9274},
  note     = {Selected Papers from NUMDIFF-12},
  abstract = {By definition algebraic stability of general linear methods is characterized by the existence of a weight matrix G leading to semi-definiteness of a 2 × 2 block test matrix depending on the coefficient matrices of the method. A congruence transformation is presented here reducing the number of places where G appears from 5 to 2 under assumptions satisfied by many methods from literature. A further reduction is possible to a test matrix depending on one single aggregated coefficient matrix P only. Simple sufficient and sharp necessary conditions on P are discussed. With these many algebraically stable implicit two-step peer methods with 3 stages and order 2 are constructed. Finally relations to Riccati equations and a generalized eigenvalue problem of Hill are discussed. },
  doi      = {https://doi.org/10.1016/j.apnum.2012.06.005},
  groups   = { General linear method},
  keywords = {Algebraic stability, General linear methods, Implicit peer two-step methods, Matrix Riccati equations },
  url      = {http://www.sciencedirect.com/science/article/pii/S0168927412000918},
}

@Article{Albrecht1996,
  author   = {Peter Albrecht},
  title    = {The common basis of the theories of linear cyclic methods and Runge-Kutta methods},
  journal  = {Applied Numerical Mathematics},
  year     = {1996},
  volume   = {22},
  number   = {1–3},
  pages    = {3 - 21},
  issn     = {0168-9274},
  note     = {Special Issue Celebrating the Centenary of Runge-Kutta Methods},
  abstract = {This contribution is a survey of results on general linear methods. Both classical theories, Dahlquist's theory of multistep methods as well as Butcher's theory of Runge-Kutta methods, are extended in such a way that a common notation and uniform concepts become possible. The distinction between linear and nonlinear methods for \{ODEs\} vanishes in favour of a common approach to different types of (composite) linear methods. },
  doi      = {https://doi.org/10.1016/S0168-9274(96)00046-3},
  groups   = { General linear method},
  url      = {http://www.sciencedirect.com/science/article/pii/S0168927496000463},
}

@Article{Abdi2015,
  author   = {A. Abdi and G. Hojjati},
  title    = {Implementation of Nordsieck second derivative methods for stiff \{ODEs\}},
  journal  = {Applied Numerical Mathematics},
  year     = {2015},
  volume   = {94},
  pages    = {241 - 253},
  issn     = {0168-9274},
  abstract = {Abstract It is the purpose of this paper to study the construction and implementation of Nordsieck second derivative methods for the numerical integration of stiff systems of first order ordinary differential equations. We construct L-stable methods of order p = s + 1 , where s is the number of internal stages, and stage order q = p . The implementation issues including the starting procedures, stage predictors, local error estimation and the changing stepsize are examined. Numerical experiments with methods of orders three and four indicate reliability of the error estimates and efficiency of the methods in a variable stepsize environment. },
  doi      = {https://doi.org/10.1016/j.apnum.2015.04.002},
