@Article{Archid2014,
  author    = {Archid, Atika and Bentbib, Abdeselem Hafid},
  title     = {Approximation of the matrix exponential operator by a structure-preserving block Arnoldi-type method},
  journal   = {Applied Numerical Mathematics},
  year      = {2014},
