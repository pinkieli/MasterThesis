  groups                     = {Unconditionally Stable Explicit Method},
  journal-iso                = {{J. Eng. Mech.}},
  keywords                   = {{computation; impulsive loads; structural dynamics; discontinuities}},
  keywords-plus              = {{TIME HISTORY ANALYSIS; STRUCTURAL DYNAMICS; INTEGRATION; DISSIPATION; ALGORITHMS; STABILITY}},
