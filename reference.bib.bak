@Article{Bra2017,
  author    = {Bra, Michal and Cardone, Angelamaria and Jackiewicz, Zdzislaw and Welfert, Bruno},
  title     = {Order reduction phenomenon for general linear methods},
  journal   = {Applied Numerical Mathematics},
  year      = {2017},
  volume    = {119},
  pages     = {94 - 114},
  issn      = {01689274},
  note      = {General linear methods;Order conditions;Order reduction;Robinson;Stiff differential system;},
  language  = {English},
  abstract  = {The order reduction phenomenon for general linear methods (GLMs) for stiff differential equations is investigated. It turns out that, similarly as for standard Runge&ndash;Kutta methods, the effective order of convergence for a large class of GLMs applied to stiff differential systems, is equal to the stage order of the method. In particular, it is demonstrated that the global error &#8214;e<sup>[n]</sup>&#8214; of GLMs of order p and stage order q applied to the Prothero&ndash;Robinson test problem y<sup>&prime;</sup>(t)=&lambda;(y(t)&minus;&phi;(t))+&phi;<sup>&prime;</sup>(t), t&isin;[t<inf>0</inf>,T], y(t<inf>0</inf>)=&phi;(t<inf>0</inf>), is O(h<sup>q</sup>)+O(h<sup>p</sup>) as h&rarr;0 and h&lambda;&rarr;&minus;&infin;. Moreover, for GLMs with Runge&ndash;Kutta stability which are A(0)-stable and for which the stability function R(z) of the underlying Runge&ndash;Kutta methods, (i.e., the corresponding RK methods which have the same absolute stability properties as the GLMs), is such that R(&infin;)&ne;1, the global error satisfies &#8214;e<sup>[n]</sup>&#8214;=O(h<sup>q+1</sup>)+O(h<sup>p</sup>) as h&rarr;0 and h&lambda;&rarr;&minus;&infin;. These results are confirmed by numerical experiments. &copy; 2017 IMACS},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  groups    = {J.C. Butcher, General linear method},
  key       = {Runge Kutta methods},
  keywords  = {Differential equations;Linear stability analysis;Stability;},
  url       = {http://dx.doi.org/10.1016/j.apnum.2017.04.001},
}

