  year = {2014},
  keywords = {Algorithms,Roads and streets,Entropy,Gabor filters},
  annote = {Boundary detection algorithms;Complex geometric structure;Cue integration;Entropy measure;Mahalanobis distances;Sequential-covering algorithms;Studentized residuals;Texture characteristics;}
}

