@article{Tamma2003b,
  title = {Algorithms by Design : Issues and New Perspectives for Computational Dynamics},
  abstract = {This chapter describes new avenues for designing dynamic computational algorithms based on the desired algorithmic properties encompassing linear multistep methods (LMS) methods. A particular sub-class of algorithms, which only involves a single equation system with no more than a single system solve at each time step pertaining to the Type 3 k 1 (where k is the number of unknowns participating in the algorithms and the subscript 1 indicates a single solve) and termed as generalized integration operators. An optimal numerical dissipation and dispersion (U1-V0) algorithm is derived to illustrate the approach of algorithm by design. Most of the existing time integration algorithms over the past several years are part of the overall framework, however, issues that have not been addressed to-date and new perspectives for designing computational dynamics are also discussed in the chapter.},
  journal = {Computational Fluid \& Solid Mechanics},
  author = {Tamma, Kumar K. and Zhou, Xiangmin and Kanapady, Ramdev},
  year = {2003},
  keywords = {printed},
  pages = {2148--2151}
}

@article{Tamma2001b,
  title = {The {{Time Dimension}}, {{Computational Algorithms}}, and {{Heat Transfer Computations}}},
  abstract = {For the analysis of problems encompassing linear transient heat conduction involving the time dimension, the present exposition describes the evolution of and synthesis leading to the general framework and design of computational algorithms. The fundamental theoretical basis emanates from a generalized time weighted philosophy, and characterization as outline here pertains to that which not only permits the general classification of time discretized operators to be established, but also provides the underlying basis for their subsequent design via discrete numumerically assigned [DNA] algorithmic markers that uniquely characterize computational algorithms. A generalized stability and accuracy limitation theorem is also highlighted for transient algorithms encompassing heat conduction problems.},
  journal = {International Centre for Heat and Mass Transfer},
  author = {Tamma, Kumar K. and Zhou, X. and Sha, D.},
