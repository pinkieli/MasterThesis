@article{Christensen1989a,
  title = {A Method for Analyzing Penetration Resistance Data},
  volume = {13},
  issn = {0167-1987},
  doi = {https://doi.org/10.1016/0167-1987(89)90040-8},
  abstract = {Analysis and data interpolation of penetration resistance (CI) are complicated by the effects of soil water content (\texttheta) and bulk density (Bd). Analysis is also complicated by the serial correlation in error over depths, which results in standard errors for hypothesis testing which are incorrect. A method is presented that attempts to normalize the effects of water content and bulk density by using a covariate analysis to adjust the \{CI\} at each depth to a reference value of \texttheta{} and Bd. This adjustment works as long as there is a relationship between \{CI\} vs. \texttheta{} and Bd. A time-series analysis is used to eliminate the serial correlation of errors when separating treatments across depths. This is done by using a first- or second-order data transformation based on residuals and a general linear method of analysis, the parameter estimates obtained are unbiased and standard errors associated with these estimates are correct.},
  number = {1},
  journal = {Soil and Tillage Research},
  author = {Christensen, N. B. and Sisson, J. B. and Barnes, P. L.},
  year = {1989},
  pages = {83 -- 91}
}

@article{Bouhamidi2014b,
  title = {A Meshless {{RBF}} Method for Computing a Numerical Solution of Unsteady {{Burgers}}'-Type Equations},
  volume = {68},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  issn = {08981221},
  abstract = {In this paper, we discuss a meshless method for the computation of a numerical solution of unsteady coupled Burgers'-type equations which are, in the most general case, nonlinear partial differential equations. Our approach is based on the interpolation of the solution by radial basis functions (RBFs) and is independent of the geometry of the domain. The existence and unicity of such of an interpolant will be established by the mean of linear algebra arguments. This spatial discretization transforms the initial problem into a system of ordinary differential equations (ODEs). We give a simple formulation of implicit Runge-Kutta (IRK) schemes that can be used to numerically solve this system of ODEs. The main difficulty in the implementation of an IRK method lies in the fact that it leads to a potentially large linear or nonlinear system that has to be solved at each time step. A matrix-GMRES approach, coupled with a Newton based linearization technique in the nonlinear case, will be used to numerically solve these systems. Some numerical examples will be provided to illustrate the performance of our approach. \&copy; 2014 Elsevier Ltd. All rights reserved.},
  language = {English},
  number = {3},
  journal = {Computers and Mathematics with Applications},
  author = {Bouhamidi, A. and Hached, M. and Jbilou, K.},
  year = {2014},
  keywords = {Ordinary differential equations,Runge Kutta methods,Linear algebra,Interpolation,Newton-Raphson method,Radial basis function networks},
  pages = {238 -- 256},
  annote = {Burgers equations;Mesh-less methods;Newton's methods;Projection method;Radial basis functions;

Burgers equations;Mesh-less methods;Newton's methods;Projection method;Radial basis functions;}
}

@article{Scacchi2008b,
  title = {A Hybrid Multilevel {{Schwarz}} Method for the Bidomain Model},
