@article{VanWieren1997a,
  title = {Using Diagonally Implicit Multistage Integration Methods for Solving Ordinary Differential Equations},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  abstract = {The use of the recently discovered DIMSIM (Diagonally IMplicit Multi-Step Integration Method) class of general linear methods for the numerical solution of ordinary differential equations has been investigated with the purpose of developing software based on these methods. DIMSIMs have high stage order and provide a Nordsieck vector at each integration step with essentially no additional computational cost. The implementation advantages of these features were extensively explored. Simplified procedures are developed for deriving parameters used in the calculation of the Nordsieck vector, interpolation, and error estimation. A continuous interpolant based on the Nordsieck vector and requiring no additional function evaluations has been developed that exists for all DIMSIMs. An automatic starting procedure has been developed and successfully tested. Second and fifth order adaptive explicit solvers DIMEX2 and DIMEX5 were developed in FORTRAN 77 and were competitive in testing against established codes on DETEST. A FASAL (First Approximately Same As Last) implementation approach was developed which takes advantage of high stage order to eliminate one internal stage calculation per step. It is shown that FASAL implementations require separate stability analyses. New Richardson and companion method approaches to error estimation are proposed that also use high stage order and require essentially no additional computational cost. Heuristic estimates for stiff problems for both second and fifth order implicit methods were developed based on a Richardson approach. Alternative predictor-corrector DIMSIM implementations were analyzed. Finally, prototype second and fifth order adaptive stiff solvers were developed and tested successfully. ProQuest Subject Headings: Mathematics, Computer science. \&copy; Citation reproduced with permission of ProQuest LLC.},
  language = {English},
  journal = {ProQuest Dissertations and Theses Global},
  author = {Van Wieren, Jack Martin},
  year = {1997},
  keywords = {Integration,Ordinary differential equations,Integral equations,Numerical methods,Differential equations,Interpolation,Estimation,Mathematical techniques,Computer science},
  annote = {Computational costs;Diagonally implicit;General linear methods;Implementation approach;Integration method;Predictor corrector;Simplified procedure;Starting procedures;}
}

@article{Gear1965a,
