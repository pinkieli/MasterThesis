@article{ZhuZhiDa1980a,
  title = {非线性结构动力分析的拟静力$\beta$-$\Theta$数值计算法及其应用},
  volume = {6},
  abstract = {本文提出一种非线性结构动力分析的拟静力$\beta$\textemdash\texttheta数值计算法.该法中包函三个选择最佳稳定性和精度的待定参数$\delta$、$\beta$、\texttheta值,只要给它们以相应的值,就可以分别退化出与目前广泛应用的克洛夫(Clough)的拟静力法,威尔逊(Wilson)的\texttheta法,纽马克(Newmark)的$\beta$法,潼泽春男的改进的拟静力法相当的等效的拟静力增量法,同时,在该法中还考虑了阻尼矩阵的形式.按拟静力$\beta$\textemdash\texttheta法编成的电算程序在实际应用中,使用者根据需要可以通过$\delta$、$\beta$、\texttheta值的选择,随时选用所需要的数值计算法. 此外,在文中还给出了拟静力$\beta$\textemdash\texttheta法在串联多自由度结构中非线性地震反应计算的详细公式,并已编成电算程序算出例子.},
  number = {3},
  journal = {北京工业大学学报},
  author = {{朱志达}},
  year = {1980},
  keywords = {动力分析,非线性结构,杆件,恢复力曲线,拟静力法,纽马克,时间增量,数值计算法,威尔逊,阻尼矩阵},
  pages = {66--81}
}

