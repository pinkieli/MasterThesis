@article{Towler1979a,
  title = {On Comparing the Accuracy of Some Finite Difference Methods for Parabolic Partial Differential Equations},
  volume = {14},
  issn = {1097-0207},
  doi = {10/bd8bmw},
  abstract = {Methods for comparing the accuracy of numerical methods for the solution of parabolic partial differential equations are briefly surveyed. Criteria for a more systematic comparison are proposed and applied to an accuracy comparison of the classical and the modified Saul'yev methods and the Crank\textendash{}Nicholson method on a simple linear problem. The results obtained, which include observation of the existence of optimal spatial mesh size for the two Saul'yev methods, are discussed.},
  number = {7},
  journal = {International Journal for Numerical Methods in Engineering},
