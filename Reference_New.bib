  abstract = {ABSTRACT Using the theory of Butcher series (rooted trees, elementary differentials,...) the order conditions of r-step s-stage multivalue methods for y ' =f(x,y) are derived. In order to solve the resulting nonlinear system for the method's coefficients the author introduces simplifying assumptions which generalize the well-known conditions B(p), C($\eta$), D($\xi$) for Runge-Kutta methods (r=1) to the case r$>$1. As an application, methods of optimal order 2s+r-1 are constructed. It is conjectured that for every r and s there exist methods of order 2s+r-1 which are zero-stable.},
  number = {1},
  journal = {Ima Journal of Numerical Analysis},
  author = {Burrage, Kevin},
  year = {1988},
  keywords = {rank5},
  pages = {43--69}
}

