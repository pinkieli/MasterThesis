  keywords = {Interpolation,Fracture,Geodynamics,Tectonics},
  pages = {818 -- 832},
  annote = {Adaptive shading;B-spline;Egypt;Segment grouping;Segment tracing;Tectonic model;}
}

@inproceedings{Askarpour2012a,
