@article{Sheinman1980a,
  title = {Dynamics of Complex Shells of Revolution with Arbitrary Stiffness and Density Distributions},
  volume = {15},
  issn = {1097-0207},
  doi = {10/btqcxt},
  abstract = {A general analytical and numerical procedure, based on the linear theory, is outlined for the elastic stress and deflection analysis of complex shells of revolution with arbitrary stiffness and density distributions, under arbitrary static and dynamic loads. The equations of motion admit shear deformation and rotary inertia. A numerical solution is obtained by recourse to Fourier expansion for the circumferential direction, to finite differences for the meridonial direction, and to Houbolt's method for the time domain. Numerical results are presented.},
