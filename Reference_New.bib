@article{Burrage1994a,
  title = {A Class of Variable-Step Explicit {{Nordsieck}} Multivalue Methods},
  abstract = {ABSTRACT Existing integrators for non-stiff initial value problems often use Adams, Runge-Kutta or extrapolation methods. Few integrators based on explicit multivalue methods have been developed, even though their generality can potentially lead to improved performance. We present exploratory results for a class of variable-step, three-step, three-stage, explicit Nordsieck multivalue methods of order six. The methods use the order six formula to advance approximations to the solution and scaled first derivatives from the previous three steps. An embedded order five formula is used to form the local error estimate. The methods have stage order five which enables us to construct a local continuous extension of order five without performing extra derivative evaluations. Our numerical results show, despite the exploratory nature of our work, that the new methods are competitive with (5,6) explicit Runge-Kutta pairs. Key words: initial value, multivalue method, multistep Nordsieck, variable-step AMS(MOS) Subject Classification: 65L05 1.},
