  author = {Tan, K.H. and Fung, T.C. and Lok, T.S.},
  year = {1993},
  keywords = {Analysis,Design,Structural panels,Steel structures},
  pages = {253 -- 258},
  annote = {Sandwich panels;Thick plate analogy;}
}

@article{Fung1999f,
