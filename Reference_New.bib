  booktitle = {Proceedings - 43rd {{Annual Conference}} on {{Information Sciences}} and {{Systems}}, {{CISS}} 2009},
  author = {Xiong, Yuan and Iglesias, Pablo A.},
  year = {2009},
  keywords = {Dynamics,Splines,Curve fitting,Time series,Biochemistry,Bioinformatics,Cell culture,Characterization,Frequency estimation,Image segmentation,Imaging systems,Information science,Mammals,Motion pictures,Signaling},
  pages = {369 -- 370},
  annote = {Alternating cycle;Amoeboid locomotion;Amoeboid motility;Angle distribution;Auto regressive process;Automated methods;Body orientation;Boundary curves;Boundary variations;Branch pruning;Cell bodies;Cell boundary;Cell shapes;Cell strains;Cellular components;Cellular morphology;Chemoattractants;Chemotaxis;Complex cellular process;Connected lines;Cytoskeletal dynamics;Dictyostelium cells;Dictyostelium discoideum;Differential interference contrast;Global informations;High frequency HF;Image noise;In-cell;Least square estimation;Local curvature;Mammalian cell lines;Manual methods;Medial axis transforms;Molecular species;Morphological dynamics;Morphological image processing;Morphological information;Negative flows;Phase-contrast;Planar shape;Pseudopodia;Quadratic B-spline curves;Second orders;Shape difference;Shape information;Shape space;Signaling networks;Signaling pathways;Skeletonization;Spatial fluctuation;Splitting rates;Stochastic features;Subjective judgement;Temporal dynamics;Track dynamics;Tumor cells;}
}

