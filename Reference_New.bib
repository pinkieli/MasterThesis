@article{Fangzong2017a,
  title = {Fast Algorithm for Structural Dynamics Problems Using Differential Quadrature Method and \$\$\{$\backslash$backslashvarvec\{\vphantom{\}\}}{{V}}\vphantom\{\}\vphantom\{\}\$\$ {{V}} -Transformation},
  volume = {36},
  issn = {1807-0302},
  doi = {10/gcg27g},
  abstract = {The differential quadrature method (DQM) has been widely used for structural dynamics problems computation. Traditional DQM usually adopts simultaneous discretization in multi-grid points, and then all variables are solved simultaneously. Obviously, this will result in the curse of dimensionality problem for large-scale systems. In this paper, on the basis of the multi-stage high-order time domain DQM, a fast numerical calculation method for large-scale structural dynamics problems based on \$\$\{$\backslash$backslashvarvec\{V\}\}\$\$ V -transformation is proposed. Using the \$\$\{$\backslash$backslashvarvec\{V\}\}\$\$ V -transformation possessed by the weighting coefficient matrix of DQM, the whole Jacobian matrix equations involved in the traditional approach of DQM can be decoupled into blocks; thus, the multi-stage block recursive method is derived. Numerical experiments show that: even using 2s times step size of the Newmark method, the computational accuracy of DQM is about 2\textendash{}3 orders of magnitude higher than that of the Newmark method. Furthermore, three different scale systems are used for time test and the results show that the multi-stage block recursive method can obtain high speedup ratio, which can significantly improve the computational efficiency of large-scale structural dynamics problems.},
  number = {4},
  journal = {Computational and Applied Mathematics},
  author = {Fangzong, Wang and Xiaobing, Liao},
  month = dec,
  year = {2017},
  pages = {1577--1590}
}

@article{Lepik2014a,
  title = {Haar Wavelets {{With}} Applications},
  abstract = {ABSTRACT This textbook presents a comprehensive overview of different applications of the Haar wavelet method. As known, the simplest wavelets are the Haar wavelets which are orthogonal, piecewise constant functions. The Haar wavelet method has some preferences, such as mathematical simplicity, existence of fast algorithms, and high accuracy for a small number of grid points. Thus very often the Haar wavelet method is simpler and faster than other methods. This useful book is mainly written for students and researchers in applied mathematics, physics and engineering. The authors demonstrate the efficiency and accuracy of the Haar wavelet method by numerous examples. All computations are realized by Matlab programs. This textbook consists of 14 chapters. The first two chapters introduce the Haar wavelets. In Chapters 3-5, the Haar wavelet method is applied to the numerical solution of ordinary differential equations, integral equations, and integro-differential equations. In order to solve a linear differential equation of order n, the n-th derivative is approximated by a finite Haar wavelet expansion (with unknown wavelet coefficients). Then all derivatives of lower order are calculated by integration. Substituting these expansions into the differential equation by collocation, one obtains a linear system for the wavelet coefficients which is to solve. In the Chapters 6 and 7, some partial differential equations are solved by the Haar wavelet method. A Haar wavelet solution of fractional integral equations is described in Chapter 8. Then, in Chapter 9, Haar wavelets are applied in optimal control theory. In Chapters 10-14, Haar wavelets are used for problems in structural mechanics, such as buckling of elastic beams and vibrations of elastic beams.},
  journal = {Mathematical Engineering},
  author = {Lepik, {\"U}lo and Hein, Helle},
  year = {2014},
  pages = {7--20}
}

@article{Mahdavi2015c,
  title = {A {{Comparative Study}} on {{Optimal Structural Dynamics Using Wavelet Functions}}},
  volume = {2015},
  abstract = {Wavelet solution techniques have become the focus of interest among researchers in different disciplines of science and technology. In this paper, implementation of two different wavelet basis functions has been comparatively considered for dynamic analysis of structures. For this aim, computational technique is developed by using free scale of simple Haar wavelet, initially. Later, complex and continuous Chebyshev wavelet basis functions are presented to improve the time history analysis of structures. Free-scaled Chebyshev coefficient matrix and operation of integration are derived to directly approximate displacements of the corresponding system. In addition, stability of responses has been investigated for the proposed algorithm of discrete Haar wavelet compared against continuous Chebyshev wavelet. To demonstrate the validity of the wavelet-based algorithms, aforesaid schemes have been extended to the linear and nonlinear structural dynamics. The effectiveness of free-scaled Chebyshev wavelet has been compared with simple Haar wavelet and two common integration methods. It is deduced that either indirect method proposed for discrete Haar wavelet or direct approach for continuous Chebyshev wavelet is unconditionally stable. Finally, it is concluded that numerical solution is highly benefited by the least computation time involved and high accuracy of response, particularly using low scale of complex Chebyshev wavelet.},
  number = {3},
  journal = {Mathematical Problems in Engineering,2015,(2015-2-22)},
  author = {Mahdavi, Seyed Hossein and Razak, Hashim Abdul},
  year = {2015},
  keywords = {Engineering and Technology(General,Mathematics(General}
}

@book{Mahdavi2013b,
  title = {A Wavelet-Based Approach for Vibration Analysis of Framed Structures},
  abstract = {In this paper, an explicit time integration scheme is proposed for structural vibration analysis by using wavelet functions. Initially, the differential equation of vibration governing SDOF (single-degree of freedom) systems has been solved by wavelet operators, and later the proposed approach has been generalized for MDOF (multi-degrees of freedom) systems. For this purpose, two different types of wavelet functions have been exemplified including, complex Chebyshev wavelet functions and simple Haar wavelet functions. In the proposed approach, a straightforward formulation has been derived from the numerical approximation of response through the wavelet definition. Emphasizing on frequency-domain approximation, a simple step-by-step algorithm has been implemented and improved to calculate the response of MDOF systems. Moreover, stability and accuracy of results have been evaluated. The effectiveness of the proposed approach is demonstrated using three examples compared with some of the existing numerical integration schemes such as family of Newmark-尾, Wilson-胃 and central difference method. In all the procedures, computation time involved has also been considered. Finally, it is concluded that the vibration analysis of structures is improved by lesser computation time and high accuracy of proposed approach, particularly, in large-scaled systems.},
  publisher = {{Elsevier Science Inc.}},
  author = {Mahdavi, S. H. and Razak, H. Abdul},
  year = {2013},
  keywords = {Vibration analysis,Numerical approximation,Optimum time history analysis,Wavelet function}
}

