  keywords = {printed,COMPLEXITY,INDEXING FUNCTION,INDEXING SCHEME,LOWER BOUND,MESH-CONNECTED PROCESSOR ARRAY,PARALLEL COMPUTATION,SORTING},
  pages = {1165--1180}
}

@inproceedings{Tamma1993a,
  title = {Evaluation and Applicability of Explicit Self-Starting Formulations for Nonlinear Structural Dynamics},
  abstract = {The evaluation and applicability of an explicit self-starting procedure is described here for applications to the transient analysis of nonlinear problems. The development of the methodology follows that proposed by Tamma and Namburu (1988) for linear situations, however, in this paper emphasis is placed on recent developments which take into account material nonlinear behavior such as elastoplastic, elasto-viscoplastic behavior, although general nonlinear effects are also applicable. In comparison to traditional practices, the proposed explicit method of computation offers simplicity and computationally attractive features for linear/nonlinear structural dynamic situations. Numerical test cases relevant to the transient analysis of nonlinear problems are illustrated and the present developments strongly support the proposition for its use in practical finite element codes.},
