@article{Ruge2003c,
  title = {Evaluation of Implicit Time-Stepping Schemes by {{CG}}-Approach},
  volume = {81},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  issn = {00457949},
  abstract = {Time-domain solvers for problems in structural dynamics with some thousands of DOF's are characterized by implicit transition equations since only such codes are unconditionally numerical stable. In addition, efficiency and accuracy demand for proper time-step adaptation procedures in order to control at least the local error. However, each new time-step causes a new coefficient-matrix A and thus a new decomposition. Updating the stiffness for nonlinear systems, too, causes varying coefficient matrices. Here these continuously updated linearized transition equations are replaced by corresponding quadratic forms. This paper is restricted to symmetric positive-definite matrices A; their minimum value is approached by a straightforward iteration starting with the solution of a corresponding explicit time-stepping scheme. Implicit and explicit scheme are characterized by the same local error properties; however, they are different in principle with respect to their stability properties. Here the most popular implicit Pade\&acute;-P11$<$/inf$>$-Newmark scheme will be treated in an explicit manner. \&copy; 2003 Elsevier Ltd. All rights reserved.},
  language = {English},
  number = {30-31},
  journal = {Computers and Structures},
  author = {Ruge, P. and Dresden, T.U. and Wagner, N. and Stuttgart, U.},
  year = {2003},
  keywords = {Matrix algebra,Stiffness,Iterative methods},
  pages = {2789 -- 2796},
  annote = {Time-stepping schemes;}
}

@article{Nicholson1986a,
  title = {{{STABLE RESPONSE OF DAMPED LINEAR SYSTEMS}} - {{III}}.},
  volume = {18},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  issn = {05831024},
  abstract = {This article reviews several aspects of the response of discrete time-invariant damped linear mechanical systems governed by the equation Mx plus Cx plus Kx equals f(t). Emphasis is placed on the response bounds under impulse excitation, step force excitation, harmonic force, and harmonic base motion. Both classical and nonclassical systems are discussed. A general bound is also reported using the one-sided Lipschitz constant. Finally, the close relation between system stability and numerical stability is illustrated using Pade approximations.},
