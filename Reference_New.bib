  issn = {1096-9845},
  doi = {10/d4txn4},
  abstract = {An explicit predictor-corrector algorithm is derived from the implicit $\alpha$-method. This explicit algorithm is shown to have better stability and accuracy properties than its Newmark-based predecessor. This algorithm is then combined with the implicit $\alpha$-method, resulting in an implicit-explicit $\alpha$-method which can be effectively utilized for linear and non-linear structural dynamics calculations.},
