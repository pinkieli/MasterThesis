@article{Johan1991a,
  title = {A Globally Convergent Matrix-Free Algorithm for Implicit Time-Marching Schemes Arising in Finite-Element Analysis in Fluids},
  volume = {87},
  issn = {0045-7825},
  doi = {10/dn4k43},
  abstract = {A solution procedure for solving nonlinear time-marching problems is presented. The non-symmetric systems of equations arising from a Newton-type linearization of these time-marching problems are solved using an iterative strategy based on the generalized minimal residual (GMRES) algorithm. Matrix-free techniques leading to reduction in storage are presented. Incorporation of a linesearch algorithm in the Newton-GMRES scheme is discussed. An automatic time-increment control strategy is developed to increase the stability of the time-marching process. High-speed flow computations demonstrate the effectiveness of these algorithms.},
  number = {2-3},
  journal = {Computer Methods in Applied Mechanics and Engineering},
  author = {Johan, Z and Hughes, Tjr and Shakib, F},
  month = jun,
  year = {1991},
