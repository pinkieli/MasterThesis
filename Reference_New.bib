  year = {1998},
  keywords = {Integration,Mathematical models,Approximation theory,Differential equations,Mathematical operators,Interpolation,Convolution,Electric filters,Conformal mapping},
  pages = {399 -- 400},
  annote = {Mixed mode signal processing;Spline methods;Analog filters;}
}

@article{Perez1994a,
