@article{Hahn1991a,
  title = {A Modified {{Euler}} Method for Dynamic Analyses},
  volume = {32},
  abstract = {Abstract A modified version of the well known Euler method for numerical integration is presented, and its application in analyses of the dynamic response of structures is discussed. This modified method was first introduced in 1981, and it has been reported that it was discovered by accident. When first introduced, however, valuable information related to the accuracy, stability and merits of the method were not provided. It is shown that the modified Euler method has features that make it an attractive approach for applications in dynamic analyses of structures. Accuracy and stability analyses are made for the method by considering free-vibrational responses of linear, undamped, single-degree-of-freedom systems. The method is explicit and extremely easy to use. However, it is conditionally stable. Applications of the method are also made which illustrate its usefulness and inherent simplicity. Furthermore, comparisons of the modified Euler method and the Newmark beta method are reported which elucidate the relative merits of these methods.},
  number = {5},
  journal = {International Journal for Numerical Methods in Engineering},
  author = {Hahn, G. D.},
  year = {1991},
  keywords = {BEM,CAUCHY PRINCIPAL VALUE INTEGRAL,QUADRATIC ISOPARAMETRIC BOUNDARY ELEMENTS},
  pages = {943--955}
}

@article{Belytschko1993a,
  title = {Explicit Multi-Time Step Integration for First and Second Order Finite Element Semidiscretizations},
  volume = {108},
  abstract = {Explicit multi-time step (subcycling) integration algorithms based on nodal partitions for both first and second order systems are presented. The algorithm is identical to an earlier algorithm of the senior author, but some simplifications have been made in the actual algorithm which make it easier to implement in general purpose programs. Consistency, convergence and stability analyses of this algorithm for first order systems are given. Interpolations on the interfaces between different time step domains are studied. A general conjecture on the stability criterion for second order systems with subcycling and nodal partitions is also proposed, and some unsolved theoretical difficulties are posed.},
  number = {3},
  journal = {Computer Methods in Applied Mechanics \& Engineering},
  author = {Belytschko, T. and Lu, Y. Y.},
  year = {1993},
  pages = {353--383}
}

@article{Prakash2014a,
  title = {Computationally Efficient Multi-Time-Step Method for Partitioned Time Integration of Highly Nonlinear Structural Dynamics},
  volume = {133},
  issn = {0045-7949},
  doi = {10.1016/j.compstruc.2013.11.013},
  abstract = {An efficient and accurate method for solving large-scale problems in non-linear structural dynamics is presented. The method uses dual-Schur domain decomposition to divide a large finite element mesh into a number of smaller subdomains, which are solved independently using a suitable mesh-size and time-step to capture the local spatial and temporal scales of the problem. Continuity of the solution between subdomains is enforced by Lagrange multipliers. It is shown that the proposed method is stable, accurate and computationally more efficient than using a uniform time-step for the entire mesh. Numerical examples are presented to illustrate and corroborate these properties. (C) 2013 Elsevier Ltd. All rights reserved.},
  number = {3},
  journal = {COMPUTERS \& STRUCTURES},
  author = {Prakash, Arun and Taciroglu, Ertugrul and Hjelmstad, Keith D.},
  month = mar,
  year = {2014},
  keywords = {Structural dynamics,Time integration,Asynchronous integrators,Domain decomposition,Multi-scale,Multi-time-step},
  pages = {51--63}
}

@article{Jacquelin2017a,
  title = {Polynomial Chaos-Based Extended {{Pade}} Expansion in Structural Dynamics},
  volume = {111},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  issn = {00295981},
  abstract = {The response of a random dynamical system is totally characterized by its probability density function (pdf). However, determining a pdf by a direct approach requires a high numerical cost; similarly, surrogate models such as direct polynomial chaos expansions are not generally efficient, especially around the eigenfrequencies of the dynamical system. In the present study, a new approach based on Pad\&eacute; approximants to obtain moments and pdf of the dynamic response in the frequency domain is proposed. A key difference between the direct polynomial chaos representation and the Pad\&eacute; representation is that the Pad\&eacute; approach has polynomials in both numerator and denominator. For frequency response functions, the denominator plays a vital role as it contains the information related to resonance frequencies, which are uncertain. A Galerkin approach in conjunction with polynomial chaos is proposed for the Pad\&eacute; approximation. Another physics-based approach, utilizing polynomial chaos expansions of the random eigenmodes, is proposed and compared with the proposed Pad\&eacute; approach. It is shown that both methods give accurate results even if a very low degree of the polynomial expansion is used. The methods are demonstrated for two degree-of-freedom system with one and two uncertain parameters. Copyright \&copy; 2016 John Wiley \&amp; Sons, Ltd. Copyright \&copy; 2016 John Wiley \& Sons, Ltd.},
  language = {English},
  number = {12},
  journal = {International Journal for Numerical Methods in Engineering},
  author = {Jacquelin, E. and Dessombz, O. and Sinou, J.-J. and Adhikari, S. and Friswell, M.I.},
  year = {2017},
  keywords = {Structural dynamics,Degrees of freedom (mechanics),Polynomials,Probability density function,Dynamical systems,Frequency domain analysis,Frequency response,Uncertainty analysis},
  pages = {1170 -- 1191},
  annote = {Approximants;Frequency response functions;Polynomial chaos expansion;Polynomial chaos representation;Probability density function (pdf);Random dynamical system;Random modes;Two-degree-of-freedom system;}
}

@article{Yue2012a,
  title = {Using {{Krylov}}-{{Pade}} Model Order Reduction for Accelerating Design Optimization of Structures and Vibrations in the Frequency Domain},
  volume = {90},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  issn = {00295981},
  abstract = {In many engineering problems, the behavior of dynamical systems depends on physical parameters. In design optimization, these parameters are determined so that an objective function is minimized. For applications in vibrations and structures, the objective function depends on the frequency response function over a given frequency range, and we optimize it in the parameter space. Because of the large size of the system, numerical optimization is expensive. In this paper, we propose the combination of Quasi-Newton type line search optimization methods and Krylov-Pade\&acute; type algebraic model order reduction techniques to speed up numerical optimization of dynamical systems. We prove that Krylov-Pade\&acute; type model order reduction allows for fast evaluation of the objective function and its gradient, thanks to the moment matching property for both the objective function and the derivatives towards the parameters. We show that reduced models for the frequency alone lead to significant speed ups. In addition, we show that reduced models valid for both the frequency range and a line in the parameter space can further reduce the optimization time. \&copy; 2012 John Wiley \&amp; Sons, Ltd.},
  language = {English},
  number = {10},
  journal = {International Journal for Numerical Methods in Engineering},
  author = {Yue, Yao and Meerbergen, Karl},
  year = {2012},
  keywords = {Dynamical systems,Frequency response,Structure (composition)},
  pages = {1207 -- 1232},
  annote = {Design optimization;Krylov method;Model order reduction;Quasi-Newton optimization;Vibrations;}
}

@article{Goudreau1973a,
  title = {Evaluation of Numerical Methods in Elastodynamics},
  volume = {2},
  abstract = {Abstract One-step algorithms are compared for the direct time integration of the spatially discretized equations of linear elastodynamics. Explicit and implicit methods are evaluated for stability and accuracy in terms of the frequency spectrum of the continuum as well as discrete solution. The closed form of the discrete spectrum of a bar and beam are given to illustrate inherent errors of a spatial discretization. Additional errors of a direct time integration scheme are interpreted in terms of the discrete modes. The Newmark family of second-order difference approximations is found superior to either the original or extended Wilson methods, and to that of Houboult. Less than half as many time steps are required for comparable accuracy. If an upper bound on the highest frequency of a system is found, an optimum algorithm for a given time step can be found, rather than the usual selection of a time step for a given algorithm. The algorithmic damping admitted by the Newmark family can be controlled as a 鈥渓inear artificial viscosity鈥 to smooth shock discontinuities without the excessive damping of the Wilson and Houboult methods. The implications of these algorithms in wave propagation problems are illustrated.},
  number = {1},
  journal = {Computer Methods in Applied Mechanics \& Engineering},
  author = {Goudreau, G. L. and Taylor, R. L.},
  year = {1973},
  pages = {69--97}
}

@article{Ruge2001a,
  title = {Restricted {{Pade}} Scheme in Computational Structural Dynamics},
  volume = {79},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  issn = {00457949},
  abstract = {Restricted Pade\&acute; schemes Rmp$<$/inf$>$ where the denominator is a complete power p of a linear function have been examined analytically by No\&die;rsett. Here this method is evaluated with paying special attention to numerical aspects when solving typical finite-element systems in structural dynamics with n degrees of freedom and with essential properties like sparsety and symmetry. This paper presents an efficient implicit restricted time-stepping scheme R22$<$/inf$>$ with a final coefficient matrix of order n and a leading error term proportional to h\textsuperscript{4}. This is a significant improvement compared with the most popular Pade\&acute;-P11$<$/inf$>$-Newmark scheme with matrix order n, too, but error term h\textsuperscript{3} on the one hand and the well established Pade\&acute;-P12$<$/inf$>$-discontinuous Hamiltonian approach with error term h\textsuperscript{4} but matrix order 2 \&times; n on the other hand. The classical P11$<$/inf$>$-scheme and the new restricted R22$<$/inf$>$-scheme are compared in a modal manner and by assessing the correlation between local error estimator and global error. \&copy; 2001 Published by Elsevier Science Ltd.},
  language = {English},
  number = {20-21},
  journal = {Computers and Structures},
  author = {Ruge, P.},
  year = {2001},
  keywords = {Algorithms,Computational methods,Damping,Finite element method,Mathematical models,Error analysis,Matrix algebra,Degrees of freedom (mechanics),Computer simulation,Correlation methods,Hamiltonians,Vibrations (mechanical),Subroutines},
  pages = {1913 -- 1921},
  annote = {Structural dynamics;}
}

@article{Rouzaud2017a,
  title = {A New Multi-Frequency Approach Based on {{Pade}} Approximants for the Treatment of Transient Dynamics Problems with the Variational Theory of Complex Rays},
  volume = {392},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  issn = {0022460X},
  abstract = {Frequency-based methods were set up in order to circumvent the limits of classical finite element methods in fast dynamic simulations due to discretizations. In this approach the dynamic loading was shifted in the frequency domain by FFT, then treated by the Variational Theory of Complex Rays, and then the time response was reconstructed through an IFFT. This strategy proved to be very efficient due to the CPU VTCR very low cost. However in the case of a large loading spectrum this frequency-by-frequency approach could seriously degrade the computational performances of the strategy. This paper addresses this point by proposing the use of Pad\&eacute; approximants in order to limit the number of frequencies at which the response should be calculated. Pad\&eacute; approximation is applied to the overall VTCR system based on its frequency dependency. Finally, as simulations on a simple academic case and on a civil engineering structure show, this method is found to be very efficient for interpolating the frequency response functions of a complex structure. This is a key point to preserve the efficiency of the complete VTCR strategy for transient dynamic problems. \&copy; 2016 Elsevier Ltd},
  language = {English},
  journal = {Journal of Sound and Vibration},
  author = {Rouzaud, C. and Gatuingt, F. and Herve, G. and Dorival, O.},
  year = {2017},
  keywords = {Finite element method,Structural analysis,Frequency domain analysis,Transient analysis,Frequency response,Dynamic loads},
  pages = {170 -- 186},
  annote = {Approximants;Civil engineering structures;Frequency resolutions;Frequency response functions;Model reduction;Transient dynamic problems;Variational theory of complex rays;VTCR;}
}

@inproceedings{Sanchez2016a,
  address = {Crete, Greece},
  title = {Model Order Reduction in Structural Dynamics},
  volume = {2},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  abstract = {Frequency response analysis in structural dynamics usually requires solving large dynamical systems of the form (-\&omega;\textsuperscript{2}M + i\&omega;D + K)u(\&omega;) = f (\&omega;), which result from a FE discretization. A straightforward solution of big systems requires a high computational cost; therefore several Model Order Reduction (MOR) techniques have been developed in the last decades to obtain faster and efficient results. Between them interpolatory approaches have gained importance for solving second order dynamical systems. This work presents and compares ten MOR techniques which are suitable for structural dynamics problems. These are: Guyan-Irons Reduction, Improved Reduction System, Dynamic Reduction, Real Modal Analysis, Complex Modal Analysis, Craig-Bampton Method, and Interpolatory MOR methods like Multi-point Pad\&eacute; Approximation, the Krylov-based Galerkin Projection and the Derivativebased Galerkin Projection. A brief summary of the theoretical background is presented for each method. A first numerical example shows the applicability for damped systems and a second example shows suitability of the Interpolatory MOR methods for industrial applications, using data from a commercial FE software (ANSYS\&reg;).},
  language = {English},
  booktitle = {{{ECCOMAS Congress}} 2016 - {{Proceedings}} of the 7th {{European Congress}} on {{Computational Methods}} in {{Applied Sciences}} and {{Engineering}}},
  author = {Sanchez, Raul Rodriguez and Buchschmid, Martin and Muller, Gerhard},
  year = {2016},
  keywords = {Galerkin methods,Structural dynamics,Computational methods,Dynamics,Structural analysis,Numerical methods,Dynamical systems,Frequency response,Application programs},
  pages = {4221 -- 4247},
  annote = {Complex modal analysis;Frequency response analysis;Guyan reduction;Krylov sub spaces;Large dynamical Systems;Model order reduction;Moment-matching;Second-order dynamical systems;}
}

@article{Boutmir2004a,
  title = {{Implicit high order solvers for non linear structural dynamic problems}},
  volume = {13},
