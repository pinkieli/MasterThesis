  issn = {0253-4827},
  doi = {10/fj84gf},
  abstract = {In this paper, a new alternating group explicit-implicit (nAGEI) scheme for dispersive equations with a periodic boundary condition is derived. This new unconditionally stable scheme has a fourth-order truncation error in space and a convergence ratio faster than some known alternating methods such as ASEI and AGE. Comparison in accuracy with the AGEI and AGE methods is presented in the numerical experiment.},
  number = {9},
  journal = {APPLIED MATHEMATICS AND MECHANICS-ENGLISH EDITION},
  author = {Qing-jie, Zhang and Wen-qia, Wang},
  month = sep,
  year = {2008},
  pages = {1221--1230}
}

