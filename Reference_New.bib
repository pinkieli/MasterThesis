@article{Addison1984c,
  title = {Properties of {{Multistep Formulas Intended}} for a {{Class}} of {{Second Order Ode}}'s},
  volume = {21},
  abstract = {We analyze the stability and order properties of multistep formulas suitable for second order ODEs of the form: My" + Cy' + Ky = R(t). These formulas are actually formula-pairs, where one formula is used to obtain an approximation to y and the other to obtain an approximation to y'. We show how to transform a k-step formula-pair into a 2k-step single formula whose stability and order properties can be more easily studied. We present conditions that ensure that the formula-pair is stable at zero and infinity and we demonstrate that the stability properties of a formula-pair can differ substantially from those of its component formulas. The order analysis provides necessary and sufficient conditions on the coefficients of a formula-pair to obtain a local truncation error of order p + 2. These conditions are dependent upon the order properties of the component formulas. Consequently, given the order of the component formulas, the order of the formula-pair is easily determined.},
  number = {2},
  journal = {Siam Journal on Numerical Analysis},
  author = {Addison, C. A. and Enright, W. H.},
  year = {1984},
  pages = {327--339}
}

@article{Gear1978a,
  title = {The {{Stability}} of {{Numerical Methods}} for {{Second Order Ordinary Differential Equations}}},
  volume = {15},
  abstract = {An important characterization of a numerical method for first order ODE's is the region of absolute stability. If all eigenvalues of the linear problem y' = Ay are inside this region, the numerical method is stable. If the second order system y" = 2Ay' - By is solved as a first order system, the same result applies to the eigenvalues of the generalized eigenvalue problem 位2I - 2位 A + B. No such region exists for general methods for second order equations, but in some cases a region of absolute stability can be defined for methods for the single second order equation y" = 2ay' - by. The absence of a region of absolute stability can occur when different members of a system of first order equations are solved by different methods.},
  number = {1},
  journal = {SIAM Journal on Numerical Analysis},
