@article{Ferracina2008a,
  title = {Strong Stability of Singly-Diagonally-Implicit {{Runge}}\textendash{}{{Kutta}} Methods},
  volume = {58},
  issn = {0168-9274},
  doi = {http://dx.doi.org/10.1016/j.apnum.2007.10.004},
  abstract = {This paper deals with the numerical solution of initial value problems, for systems of ordinary differential equations, by Runge\textendash{}Kutta methods (RKMs) with special nonlinear stability properties indicated by the terms total-variation-diminishing (TVD), strongly stable and monotonic. Stepsize conditions, guaranteeing these properties, were studied earlier, see e.g. Shu and Osher [C.-W. Shu, S. Osher, J. Comput. Phys. 77 (1988) 439\textendash{}471], Gottlieb et al. [S. Gottlieb, C.-W. Shu, E. Tadmor, \{SIAM\} Rev. 43 (2001) 89\textendash{}112], Hundsdorfer and Ruuth [W.H. Hundsdorfer, S.J. Ruuth, Monotonicity for time discretizations, in: D.F. Griffiths, G.A. Watson (Eds.), Proc. Dundee Conference 2003, Report NA/217, Univ. Dundee, 2003, pp. 85\textendash{}94], Higueras [I. Higueras, J. Sci. Computing 21 (2004) 193\textendash{}223; I. Higueras, \{SIAM\} J. Numer. Anal. 43 (2005) 924\textendash{}948], Gottlieb [S. Gottlieb, J. Sci. Computing 25 (2005) 105\textendash{}128], Ferracina and Spijker [L. Ferracina, M.N. Spijker, \{SIAM\} J. Numer. Anal. 42 (2004) 1073\textendash{}1093; L. Ferracina, M.N. Spijker, Math. Comp. 74 (2005) 201\textendash{}219]. Special attention was paid to \{RKMs\} which are optimal, in that the corresponding stepsize conditions are as little restrictive as possible within a given class of methods. Extensive searches for such optimal methods were made in classes of explicit RKMs, see e.g. Gottlieb and Shu [S. Gottlieb, C.-W. Shu, Math. Comp. 67 (1998) 73\textendash{}85], Spiteri and Ruuth [R.J. Spiteri, S.J. Ruuth, \{SIAM\} J. Numer. Anal. 40 (2002) 469\textendash{}491; R.J. Spiteri, S.J. Ruuth, Math. Comput. Simulation 62 (2003) 125\textendash{}135], Ruuth [S.J. Ruuth, Math. Comp. 75 (2006) 183\textendash{}207]. In the present paper we search for methods that are optimal in the above sense, within the interesting class of singly-diagonally-implicit Runge\textendash{}Kutta (SDIRK) methods, with s stages and order p. Some methods, with 1 $\leqslant$ p $\leqslant$ 4 , are proved to be optimal, whereas others are obtained by a numerical search. We present closed-form expressions for two families of \{SDIRK\} methods (with s $\geqslant$ 3 ) which we conjecture to be optimal for p = 2 and p = 3 , respectively. Furthermore we prove, for strongly stable \{SDIRK\} methods, the order barrier p $\leqslant$ 4 . We perform numerical experiments, to compare the theoretical properties of various optimal \{SDIRK\} methods to the actual \{TVD\} properties in the solution of a nonlinear test equation, the 1-dimensional Buckley\textendash{}Leverett equation.},
  number = {11},
  journal = {Applied Numerical Mathematics},
  author = {Ferracina, L. and Spijker, M. N.},
  year = {2008},
  keywords = {Initial value problem,Method of lines (MOL),Monotonicity,Ordinary differential equation (ODE),Singly-diagonally-implicit Runge–Kutta method (SDIRK),Strong-stability-preserving (SSP),Total-variation-diminishing (TVD)},
  pages = {1675 -- 1686}
}

@article{Butcher2009b,
  title = {Order and Stability of Generalized {{Pad{\'e}}} Approximations},
  volume = {59},
  issn = {0168-9274},
  doi = {http://dx.doi.org/10.1016/j.apnum.2008.03.006},
  abstract = {Given a sequence of integers [ n 0 , n 1 , \ldots{} , n r ] , where n 0 , n r $\geqslant$ 0 and n i $\geqslant$ - 1 , i = 1 , 2 , \ldots{} , r - 1 , a sequence of r polynomials ( P 0 , P 1 , \ldots{} , P r ) is a generalized Pad{\'e} approximation to the exponential function if $\sum$ i = 0 r exp ( ( r - i ) z ) P i ( z ) = O ( z p + 1 ) , where the order of the approximation p is given by p = $\sum$ i = 0 r ( n i + 1 ) - 1 . The main result of this paper is that if 2 n 0 \&gt; p + 2 , then $\sum$ i = 0 r w r - i P i ( z ) is not the stability polynomial of an A-stable numerical method. This result, known as the Butcher\textendash{}Chipman conjecture, generalizes the corresponding result for rational Pad{\'e} approximations. The special case, formerly known as the Ehle conjecture [B.L. Ehle, A-stable methods and Pad{\'e} approximations to the exponential, \{SIAM\} J. Math. Anal. 4 (1973) 671\textendash{}680], was subsequently proved by Hairer, N{\o}rsett and Wanner [G. Wanner, E. Hairer, S.P. N{\o}rsett, Order stars and stability theorems, \{BIT\} 18 (1978) 475\textendash{}489].},
  number = {3\textendash{}4},
  journal = {Applied Numerical Mathematics},
  author = {Butcher, J. C.},
  year = {2009},
  keywords = {Stability,Generalized Padé approximations,Order,Order arrows,Order barriers,Order stars,Padé approximations},
  pages = {558 -- 567},
  annote = {Selected Papers from NUMDIFF-11}
}

