  author = {Erkorkmaz, Kaan and Altintas, Yusuf},
  year = {2005},
  keywords = {Computational methods,Errors,Integral equations,Polynomials,Interpolation,Functions,Boundary conditions,Harmonic analysis,Computer control,Computer control systems,Scheduling,Feeding},
  pages = {339 -- 349},
  annote = {Computer numeric control (CNC);Feed rates;Quintic splines;Taylor series;}
}

