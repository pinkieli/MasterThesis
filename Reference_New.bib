  author = {Aman, Rafael and Handroos, Heikki and Eskola, Tero},
  year = {2008},
  keywords = {Integration,Splines,Boundary conditions,Transients,Fluid dynamics,Braking,Drop breakup,Drops,Electric power systems,Flow simulation,Laminar flow,Orifices,Pressure drop,Speed,Thermography (temperature measurement),Turbulence},
  pages = {945 -- 961},
  annote = {Calculation speed;Computationally efficient;Cubic spline functions;Cubic splines;First derivatives;Fluid power circuits;Fluid power systems;Geometrical data;Integration time;Numerical noise;Numerical problems;Orifice flows;Orifice model;Real-time;Real-time simulations;Second derivatives;Simulation;Transition pressures;Two-regime;}
