@article{Guttel2016a,
  title = {Scaled and Squared Subdiagonal Pade Approximation for the Matrix Exponential},
  volume = {37},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  issn = {08954798},
  abstract = {The scaling and squaring method is the most widely used algorithm for computing the exponential of a square matrix A. We introduce an efficient variant that uses a much smaller squaring factor when ||A|| \&raquo; 1 and a subdiagonal Pad\&eacute; approximant of low degree, thereby significantly reducing the overall cost and avoiding the potential instability caused by overscaling, while giving forward error of the same magnitude as that of the standard algorithm. The new algorithm performs well if a rough estimate of the rightmost eigenvalue of A is available and the rightmost eigenvalues do not have widely varying imaginary parts, and it achieves significant speedup over the conventional algorithm especially when A is of large norm. Our algorithm uses the partial fraction form to evaluate the Pad\&eacute; approximant, which makes it suitable for parallelization and directly applicable to computing the action of the matrix exponential exp(A)b, where b is a vector or a tall skinny matrix. For this problem the significantly smaller squaring factor has an even more pronounced benefit for efficiency when evaluating the action of the Pad\&eacute; approximant. Copyright \&copy; by SIAM. Unauthorized reproduction of this article is prohibited.},
