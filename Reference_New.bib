@article{Kwon2017a,
  title = {A Non-Oscillatory Time Integration Method for Numerical Simulation of Stress Wave Propagations},
  volume = {192},
  issn = {0045-7949},
  abstract = {Abstract When the compressive loads are dominant in a composite structure, a tensile stress may be induced owing to the propagation of a stress wave and the interaction between an incident wave and a reflection wave, thus leading to the occurrence of cracks. Therefore, stress wave have a significant effect on the life of composite structures. In this study, a four sub-step explicit time integration scheme is proposed for solving stress wave propagation problems. This method builds on the fourth-order central difference method and a high-order derivative term to minimize the numerical oscillation. The proposed scheme possesses a first-order accuracy in the case of undamped and damped systems. Stability, accuracy, and dispersion of the proposed explicit direct time integration scheme are analyzed. Furthermore, the performance of this scheme is illustrated by the solution of a stress wave propagation and wave reflection in a one-dimensional impact problem and two-dimensional scalar wave propagation.},
  number = {Supplement C},
  journal = {Computers \& Structures},
  author = {Kwon, Sun-Beom and Lee, Jae-Myung},
  year = {2017},
  keywords = {printed,Finite elements,Explicit time integration,Numerical dispersion,Numerical oscillation,Stability condition,Stress wave propagation},
  pages = {248 -- 268}
}

@inproceedings{Tornabene2007a,
  title = {The {{Generalized Differential Quadrature Method}} for {{Solving Initial}}-Value {{Problems}} in {{Linear Dynamics}}},
  abstract = {ABSTRACT In this paper, the Generalized Differential Quadrature (GDQ) Method1-2 is applied to solve initial value problems in linear dynamics. By using the GDQ technique, the numerical statement of the problem does not pass through any variational formulation, but deals directly with the governing equations of motion3-7. The purpose is to obtain computationally efficient higher-order methods for solving time integration problems. The total time interval is discretized in time steps and the GDQ method is applied to solve the initial value problem within each time step. Various choices for the number of grid points and their distribution are analyzed and discussed. At each time interval, a linear algebraic equation system has to be solved. A simple and efficient implementation scheme is presented. To demonstrate the performance of the proposed algorithm, a wide numerical evaluation is carried out on typical test problems8-10. Accuracy and stability are studied when the number of sampling points and the grid distribution are varied.},
  booktitle = {International {{Symposium}} on {{Recent Advances}} in {{Mechanics}}, {{Dynamical Systems}} and {{Probability Theory}}},
  author = {Tornabene, Francesco and Ubertini, Francesco and Viola, Erasmo},
  year = {2007}
}

@article{Mancusob,
  title = {Effective Implementation of a Time Discontinuous {{Galerkin}} Method},
  abstract = {Abstract This work deals with time integration methods for linear dynamics. The semidiscrete equations are generally derived from standard finite element procedures and only a few low-frequency modes are accurate approximations of the underlying continuous problem. Thus, the quality of simulations depends crucially on the accuracy properties of the time integration method in the low-frequency modes, as well as on the dissipative properties in the high-frequency ones. The Time Discontinuous Galerkin (TDG) method possesses these desirable properties and has been successfully applied to linear dynamics (see e.g. (1)). Despite its good properties, it typically leads to systems of coupled equations which are larger than the original one, so that standard implementations are very demanding for both storage and computational work. Thus, developing effective implementations plays a key role in the practical application of the method. The purpose of this work is to establish an efficient iterative procedure to implement the TDG method. Differently from previous implementations (see e.g. (2) and (3)), the present procedure is designed to generate a sequence of updated values for displacements and velocities, by searching for the TDG solution while preserving the TDG stability and dissipative properties at each iteration. Improving only accuracy is demanded to iterative corrections. The idea for the iterative scheme stems from the implementation of the N酶rsett methods (4). The scheme is established so to obtain at the first iteration a N酶rsett approximant with the target stability and dissipative properties. The resultant procedure is simple and offers several advantages. Remarkable computational savings are achieved since the iterative corrections are performed using the same iteration matrix, which retains the structure of the original system and can be factorized only once for each fixed time step size. No more than two iterations are needed to obtain the same overall accuracy of the TDG method. Second order accuracy is achieved at the first iteration and third order accuracy at the second one. References},
  author = {Mancuso, Massimo and Ubertini, Francesco}
}

@article{Ruge2003b,
  title = {Evaluation of Implicit Time-Stepping Schemes by {{CG}}-Approach \ding{73}},
  volume = {81},
  abstract = {Here these continuously updated linearized transition equations are replaced by corresponding quadratic forms. This paper is restricted to symmetric positive-definite matrices ja:math ; their minimum value is approached by a straightforward iteration starting with the solution of a corresponding explicit time-stepping scheme. Implicit and explicit scheme are characterized by the same local error properties; however, they are different in principle with respect to their stability properties. Here the most popular implicit Pad茅- ja:math -Newmark scheme will be treated in an explicit manner.},
  number = {30\textendash{}31},
  journal = {Computers \& Structures},
  author = {Ruge, P. and Dresden, T. U. and Wagner, N. and Stuttgart, U.},
  year = {2003},
  keywords = {building structure,random vibration,stochastic averaging,stochastic dynamical programming,stochastic optimal control},
  pages = {2789--2796}
}

@article{Mancuso2001a,
  title = {Collocation Methods with Controllable Dissipation for Linear Dynamics},
  volume = {190},
  abstract = {This paper introduces a unified set of collocation methods for linear dynamics in which a parameter is used to control the position of the collocation points. In this manner, both A-stable algorithms of order 2 p and L-stable algorithms of order 2 p鈭1 are derived from collocation polynomials of degree p. In addition, algorithms with intermediate accuracy and stability properties are made available. The effects of varying the algorithmic parameter are investigated with particular reference to the numerical dissipation of spurious high-frequency modes. Numerical tests are reported which support the theoretical analysis and demonstrate the performance of the proposed algorithms.},
  number = {28},
  journal = {Computer Methods in Applied Mechanics \& Engineering},
  author = {Mancuso, Massimo and Ubertini, Francesco},
  year = {2001},
  keywords = {16,35},
  pages = {3607--3621}
}

@article{Mancusoc,
  title = {Effective Higher-Order Methods for Linear Dynamics},
  abstract = {ABSTRACT This work presents a simple and efficient iterative procedure aimed at improving the properties of N酶rsett methods for linear dynamics towards those of Pad茅 methods. The procedure is used to implement a set of higher-order methods with controllable dissipation, recently presented by the authors. Some numerical tests demonstrate the effectiveness of the proposed implementation. Competitive time stepping methods for finite element analysis in linear dynamics should possess certain peculiar attributes. In addition to unconditional stability, the salient properties are higher-order accuracy and controllable dissipation in the spurious high modes. Higher-order accuracy is crucial for long-term simulations in order to avoid an excessive accumulation of error. Moreover, some amount of dissipation in the higher modes is desirable in order to damp out their response, as it is well known that only a few low-frequency modes of the semidiscrete problem are accurate approximations to the modes of the original continuous problem. Indeed, the dissipative properties of most of the established A-and L-stable methods can be unsatisfactory. A-stable methods can damp out the high-frequency modes too slowly and spurious effects can show up. On the other hand, L-stable methods can introduce excessive dissipation in the low-mode response and this can affect the accuracy especially in long-term simulations. Therefore, a specific algorithmic parame-ter should be available to control the amount of numerical dissipation according to the importance of the spurious modal components.},
  author = {Mancuso, M. and Ubertini, F.}
}

@book{Kunthong2005a,
  title = {An Efficient Solver for the High-Order Accurate Time-Discontinuous {{Galerkin}} ({{TDG}}) Method for Second-Order Hyperbolic Systems},
  abstract = {An efficient predictor/multi-corrector algorithm for the high-order accurate time-discontinuous Galerkin (TDG) method is presented. The algorithm overcomes the demanding storage and computational effort of the direct solution of the TDG matrix equations. The success of the block matrix iterative solution method is a result of a specific form of time approximation. An analysis shows that the algorithm needs only a few iteration passes with a single matrix factorization of size equal to the number of spatial degrees-of-freedom to reach the 2 p + 1 order of accuracy of the parent TDG solution obtained from a direct solve. For linear ( p = 1 ) and quadratic ( p = 2) approximations in time, each iteration pass retains the unconditional and asymptotic stability of the TDG parent solution. Numerical results demonstrate the efficiency and accuracy of the time-integration algorithm over second-order accurate single-step/single-solve (SS/SS) methods.},
  publisher = {{Elsevier Science Publishers B. V.}},
  author = {Kunthong, Prapot and Thompson, Lonny L.},
  year = {2005},
  keywords = {high-order accurate time stepping schemes,predictor/multi-corrector algorithms,second-order hyperbolic equation,time-discontinuous Galerkin method,time-integration methods}
}

@article{Mancuso2002a,
  title = {The {{N{\o}rsett}} Time Integration Methodology for Finite Element Transient Analysis},
  volume = {191},
  abstract = {This paper presents a set of methods for time integration of problems arising from finite element semidiscretizations. The purpose is to obtain computationally efficient methods which possess higher-order accuracy and controllable dissipation in the spurious high modes. The methods are developed and analysed by a general collocation methodology which leads to the class of N酶rsett approximants. An algorithmic parameter is used to achieve an effective control over numerical dissipation. Moreover, a simple and efficient implementation scheme is presented. At each time step, algorithms based on p-order collocation polynomials require the solution of p sets of linear algebraic equations with the same coefficient matrix. In this way, a single factorization is needed and no transformations are required to recover the approximate solution at the end or within the time interval. To demonstrate the performance of the proposed algorithms, a wide experimental evaluation is carried out on typical test problems in finite element transient analysis.},
  number = {29},
  journal = {Computer Methods in Applied Mechanics \& Engineering},
  author = {{Mancuso} and {Massimo} and {Ubertini} and {Francesco}},
  year = {2002},
  keywords = {Transient analysis,Collocation methodology,Controllable dissipation,Higher-order algorithms,Nørsett approximants},
  pages = {3297--3327}
}

@article{Tornabene2015a,
  title = {Strong {{Formulation Finite Element Method Based}} on {{Differential Quadrature}}: {{A Survey}}},
  volume = {1},
  abstract = {Abstract A survey of several methods under the heading of SFEM (Strong Formulation Finite Element Method) is presented. These approaches are distinguished from classical termed Weak Formulation Finite Element Method (WFEM). The main advantage of the SFEM is that it uses differential quadrature method (DQM) for the discretization of the equations and the mapping technique for the coordinate transformation from the Cartesian to the computational domain. Moreover the element connectivity is performed by using kinematic and static conditions so that displacements and stresses are continuous across the element boundaries. Numerical investigations integrate this survey by giving graphical details on the subject.},
  number = {2},
  journal = {Applied Mechanics Reviews},
  author = {Tornabene, Francesco and Fantuzzi, Nicholas and Ubertini, Francesco and Viola, Erasmo},
  year = {2015},
  keywords = {Errors,Polynomials,Finite element methods,Kinematics,Stress,Approximation,Boundary-value problems,Shapes,Spectra (Spectroscopy,Structural elements (Construction},
  pages = {145--175}
}

@article{Kim2014a,
  title = {A {{Cross Weighted}}-{{Residual Time Integration Scheme}} for {{Structural Dynamics}}},
  volume = {14},
  abstract = {In this article, we develop a novel stable time integration scheme for the transient analysis of structural dynamics problems. A second-order (in time) differential operator equation (e.g. obtained after finite element discretization in space) is written as a pair of first-order equations in terms of displacements and velocities. Then the solution is sought by minimizing the inner product of the residuals in the two equations (an unconventional approach) over typical time interval to obtain a symmetric set of algebraic equations involving displacements and velocities at two subsequent intervals. The new time integration scheme is termed the cross weighted-residual (CWR) time integration scheme because each of the two residuals takes the other one as a weight function in the minimization. The CWR time integration scheme is developed by using a uniform linear time approximation of the displacement and velocity fields to yield only a single step time integration scheme, which is comparable to the Newmark family of time integration scheme. A reduced integration technique is used to prevent velocity locking, which is caused by linear approximation of both the displacement and velocity fields. For the verification of the consistency and the stability, the CWR time integration scheme is tested with single-degree as well as multi-degree of freedom problems. The scheme performs extremely well compared with those of the well-known Newmark family of time integration schemes.},
  number = {06},
  journal = {International Journal of Structural Stability \& Dynamics},
  author = {Kim, Wooram and Park, Sang-Shin and Reddy, J. N.},
  year = {2014},
  keywords = {finite element method,structural dynamics,Time integration schemes,weighted-residual method},
  pages = {1450023--}
}

@article{Razavi2007a,
  title = {A {{Weighted Residual Parabolic Acceleration Time Integration Method}} for {{Problems}} in {{Structural Dynamics}}},
  volume = {7},
  abstract = {In the proposed method, the variation of displacement in each time step is assumed to be a fourth order polynomial in time and its five unknown coefficients are calculated based on: two initial conditions from the previous time step; satisfying the equation of motion at both ends of the time step; and the zero weighted residual within the time step. This method is non-dissipative and its dispersion is considerably less than in other popular methods. The stability of the method shows that the critical time step is more than twice of that for the linear acceleration method and its convergence is of fourth order.},
  number = {3},
  journal = {Computational Methods in Applied Mathematics},
  author = {Razavi, S. H and Abolmaali, A and Ghassemieh, M},
  year = {2007},
  keywords = {time integration,structural dynamics,implicit method,explicit method,numerical stability,numerical accuracy,convergence,dispersion,dissipation},
  pages = {227--238}
}

@article{Bathe2005a,
  title = {On a Composite Implicit Time Integration Procedure for Nonlinear Dynamics},
  volume = {83},
  abstract = {Transient analysis of nonlinear problems in structural and solid mechanics is mainly carried out using direct time integration of the equations of motion. For reliable solutions, a stable and efficient integration algorithm is desirable. Methods that are unconditionally stable in linear analyses appear to be a natural choice for use in nonlinear analyses, but unfortunately may not remain stable for a given time step size in large deformation and long time range response solutions. A composite time integration scheme is proposed and tested in some example solutions against the trapezoidal rule and the Wilson 胃 -method, and found to be effective where the trapezoidal rule fails to produce a stable solution. These example results are indicative of the merits of the composite scheme.},
  number = {31\textendash{}32},
  journal = {Computers \& Structures},
  author = {Bathe, Klaus J{\"u}rgen and Baig, Mirza M. Irfan},
  year = {2005},
  keywords = {Stability,Nonlinear dynamic analysis,Direct time integration},
  pages = {2513--2524}
}

