  year = {2012},
  keywords = {Algorithms,Lagrange multipliers,Frequency domain analysis,Aerospace applications,Energy efficiency,Modal analysis,Rotating disks,Turbines},
  pages = {2578 -- 2601},
  annote = {B-spline;Blade design;Commercial software;Component mode synthesis;Compressor stage;Computational effort;Contact interaction;Dry friction;Finite element models;Industrial context;Integration procedure;Low energies;Non-smooth;Nonlinear dynamical behaviors;Normal operating conditions;Numerical challenges;Numerical properties;Numerical tools;Operating condition;Parameter studies;Realistic model;Rotational velocity;Structural component;Structural interactions;Three-dimensional contact;Time marching;Time-dependent;Turbomachines;Unilateral contact;}
}

@article{Xidias2012a,
  title = {Integrating Path Planning, Routing, and Scheduling for Logistics Operations in Manufacturing Facilities},
  volume = {43},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  issn = {01969722},
  abstract = {This article considers the following manufacturing/logistics problem: an autonomous vehicle (AV) is requested to serve a set of pickup and delivery (P/D) stations in the shop floor of a modern factory providing P/D tasks while moving safely (i.e., avoiding any collision with obstacles) in its environment. A two-sided time window associated with each P/D station brackets the service time to within a specified interval. A tour for AV is considered legal if it (a) is collision free, (b) passes through each P/D station exactly once, and (c) satisfies the service time restrictions imposed by the P/D stations. A legal tour always starts and ends at a depot. The problem is thereby dual NP-hard because it combines the characteristics of path planning and those of vehicle routing and scheduling problems. The objective is to determine the shortest possible legal tour for AV. A new method is introduced to solve the problem accomplished in two successive phases: first, AV's environment is mapped into a 2D B-spline surface embedded in 3D Euclidean space using a robust geometric model. Then, the generated surface is searched using a genetic algorithm for an optimum legal tour that satisfies the requirements of the vehicle's mission. The performance of the proposed method is investigated and discussed through characteristic simulated experiments. Copyright \&copy; 2012 Taylor \&amp; Francis Group, LLC.},
  language = {English},
  number = {3},
  journal = {Cybernetics and Systems},
  author = {Xidias, Elias K. and Nearchou, Andreas C. and Aspragathos, Nikos A.},
  year = {2012},
  keywords = {Motion planning,Three dimensional,Geometry,Collision avoidance,Scheduling,Two dimensional},
  pages = {143 -- 162},
  annote = {Autonomous Vehicles;B-spline surface;Collision with obstacle;Euclidean spaces;Geometric models;Logistics operations;Manufacturing facility;NP-hard;pickup and delivery;routing;Service time;Shop floor;Simulated experiments;Time windows;Vehicle routing and scheduling;}
}

@article{Dondera2012a,
  title = {Exploring Above-Threshold Ionization of Hydrogen in an Intense x-Ray Laser Field through Nonperturbative Calculations},
  volume = {85},
  copyright = {Compilation and indexing terms, Copyright 2017 Elsevier Inc.},
  issn = {10502947},
  abstract = {This paper addresses the problem of above-threshold ionization (ATI) of hydrogen interacting with a short and intense x-ray electromagnetic field. We consider the weakly relativistic regime where the speed of the photoelectron stays well below the velocity of the light c. We solve the time-dependent Schro\&die;dinger equation (TDSE) using a spectral approach with a basis of one-electron orbitals, calculated with L2-integrable B-spline functions for the radial coordinate and bipolar spherical harmonics Ylm$<$/inf$>$ for the angular part. Retardation effects are included up to O(1/c); they induce two extra terms leading to electric quadrupole and magnetic dipole transitions. These latter terms depend on the polarization and photon momentum directions, forcing the resolution of the TDSE in a three-dimensional space. Relativistic effects [of O(1/c2)] are fully neglected. Photoelectron energy and angular distributions are obtained for photon energies ranging from 200 eV to 3 keV. We study the lower energy region of the ATI spectrum and we analyze nondipole effects, through their (l,m) components, in particular, for the lower two peaks. Although these effects are small, we show that, increasing the photon energy, the photoelectron angular distributions begin to differ significantly from the ones obtained in dipole approximation. \&copy; 2012 American Physical Society.},
  language = {English},
  number = {1},
  journal = {Physical Review A - Atomic, Molecular, and Optical Physics},
  author = {Dondera, M. and Bachau, H.},
  year = {2012},
  keywords = {Interpolation,Electromagnetic fields,Harmonic analysis,Angular distribution,Electrons,Hydrogen,Ionization,Laser optics,X ray lasers},
  annote = {Above-threshold ionization;B spline functions;Dinger equation;Dipole approximation;Electric quadrupoles;Lower energies;Magnetic dipole transition;Non-dipole effects;Non-perturbative calculations;Orbitals;Photoelectron angular distributions;Photoelectron energy;Photon energy;Photon momentum;Radial coordinates;Relativistic effects;Relativistic regime;Retardation effect;Spherical harmonics;Three dimensional space;Time-dependent;X-ray laser field;}
}

